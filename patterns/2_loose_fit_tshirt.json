{
"pattern": {
  "title": " 2 Loose Fit T-Shirt",
  "measurements": [
      "neck",
      "neck_diameter",

      "shoulder_length",

      "across_chest",
      "across_back",

      "front_highbust_height",
      "back_highbust_height",

      "bust",
      "front_bust_width",
      "front_bust_height",
      "back_bust_width",
      "back_bust_height",

      "waist",
      "front_waist_length",
      "back_waist_length",
      "side_waist_length",

      "front_neck_balance",
      "back_neck_balance",
      "front_shoulder_balance",
      "back_shoulder_balance",

      "hip",
      "back_hip_height",

      "armscye",
      "overarm_length",
      "underarm_length",
      "bicep",
      "armcap_height",
      "side_underarm_to_bust_height"

  ],
  "main": [
    {
      "id": "Back A",
      "type": "path",
      "d": [
        ["M", ["pt.a1.x","pt.a1.y"] ],
        ["L", ["pt.a3.x","pt.a3.y"] ],
        ["L", ["pt.a8.x","pt.a8.y"] ],
        ["L", ["pt.a6.x","pt.a6.y"] ],
        ["C", ["pt.a6out.x","pt.a6out.y"], ["pt.a11in.x","pt.a11in.y"], ["pt.a11.x","pt.a11.y"] ],
        ["L", ["pt.a10.x","pt.a10.y"] ],
        ["L", ["pt.a9.x","pt.a9.y"] ],
        ["C", ["pt.a9out.x","pt.a9out.y"], ["pt.a1in.x","pt.a1in.y"], ["pt.a1.x","pt.a1.y"] ],
        ["z"]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"3", "stroke":"#0000dd", "fill":"none" }
    },
    {
      "id": "Front B",
      "type": "path",
      "d": [
        ["M", ["pt.b1.x","pt.b1.y"] ],
        ["L", ["pt.b3.x","pt.b3.y"] ],
        ["L", ["pt.b8.x","pt.b8.y"] ],
        ["L", ["pt.b6.x","pt.b6.y"] ],
        ["C", ["pt.b6out.x","pt.b6out.y"], ["pt.b11in.x","pt.b11in.y"], ["pt.b11.x","pt.b11.y"] ],
        ["L", ["pt.b10.x","pt.b10.y"] ],
        ["L", ["pt.b9.x","pt.b9.y"] ],
        ["C", ["pt.b9out.x","pt.b9out.y"], ["pt.b1in.x","pt.b1in.y"], ["pt.b1.x","pt.b1.y"] ],
        ["z"]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"3", "stroke":"#0000dd", "fill":"none" }
    },
    {
      "id": "Sleeve C",
      "type": "path",
      "d": [
        ["M", ["pt.c8.x","pt.c8.y"] ],
        ["C", ["pt.c8out.x","pt.c8out.y"], ["pt.c7in.x","pt.c7in.y"], ["pt.c7.x","pt.c7.y"] ],
        ["C", ["pt.c7out.x","pt.c7out.y"], ["pt.c6in.x","pt.c6in.y"], ["pt.c6.x","pt.c6.y"] ],
        ["L", ["pt.c12.x","pt.c12.y"] ],
        ["L", ["pt.c11.x","pt.c11.y"] ],
        ["L", ["pt.c4.x","pt.c4.y"] ],
        ["C", ["pt.c4out.x","pt.c4out.y"], ["pt.c5in.x","pt.c5in.y"], ["pt.c5.x","pt.c5.y"] ],
        ["C", ["pt.c5out.x","pt.c5out.y"], ["pt.c8in.x","pt.c8in.y"], ["pt.c8.x","pt.c8.y"] ],
        ["z"]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"3", "stroke":"#0000dd", "fill":"none" }
    },
    {
      "id": "Back Neck Band D",
      "type": "path",
      "d": [
        ["M", ["pt.d1.x","pt.d1.y"] ],
        ["L", ["pt.d2.x","pt.d2.y"] ],
        ["L", ["pt.d3.x","pt.d3.y"] ],
        ["L", ["pt.d4.x","pt.d4.y"] ],
        ["z"]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"3", "stroke":"#0000dd", "fill":"none" }
    },
    {
      "id": "Front Neck Band E",
      "type": "path",
      "d": [
        ["M", ["pt.e1.x","pt.e1.y"] ],
        ["L", ["pt.e2.x","pt.e2.y"] ],
        ["L", ["pt.e3.x","pt.e3.y"] ],
        ["L", ["pt.e4.x","pt.e4.y"] ],
        ["z"]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"3", "stroke":"#0000dd", "fill":"none" }
    },
    {
      "id": "Shoulder Stay Strip F",
      "type": "path",
      "d": [
        ["M", ["pt.f1.x","pt.f1.y"] ],
        ["L", ["pt.f2.x","pt.f2.y"] ],
        ["L", ["pt.f3.x","pt.f3.y"] ],
        ["L", ["pt.f4.x","pt.f4.y"] ],
        ["z"]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"3", "stroke":"#0000dd", "fill":"none" }
    }
  ],
  "construction": [
    {
      "id": "Back Construction Lines",
      "type": "path",
      "d": [
        ["M", ["pt.backNape.x","pt.backNape.y"] ],
        ["L", ["pt.backHipCenter.x","pt.backHipCenter.y"] ],
        ["L", ["pt.backHipSide.x","pt.backHipSide.y"] ],
        ["L", ["pt.backHighbustSide.x","pt.backHighbustSide.y"] ],
        ["L", ["pt.backAcrossSide.x","pt.backAcrossSide.y"] ],
        ["L", ["pt.backShoulderTip.x","pt.backShoulderTip.y"] ],
        ["L", ["pt.backNeckSide.x","pt.backNeckSide.y"] ],
        ["L", ["pt.backNape.x","pt.backNape.y"] ],

        ["M", ["pt.backAcrossCenter.x","pt.backAcrossCenter.y"] ],
        ["L", ["pt.backAcrossSide.x","pt.backAcrossSide.y"] ],

        ["M",["pt.backHighbustCenter.x","pt.backHighbustCenter.y"] ],
        ["L", ["pt.backHighbustSide.x","pt.backHighbustSide.y"] ],

        ["M",["pt.backBustCenter.x","pt.backBustCenter.y"] ],
        ["L", ["pt.backBustSide.x","pt.backBustSide.y"] ],

        ["M",["pt.backWaistCenter.x","pt.backWaistCenter.y"] ],
        ["L", ["pt.backWaistSide.x","pt.backWaistSide.y"] ],

        ["M",["pt.backHipCenter.x","pt.backHipCenter.y"] ],
        ["L", ["pt.backHipSide.x","pt.backHipSide.y"] ]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
    },
    {
      "id": "Back Measurement Lines",
      "type": "path",
      "d": [
        ["M", ["pt.backWaistCenter.x","pt.backWaistCenter.y"] ],
        ["L", ["pt.backNeckSide.x","pt.backNeckSide.y"] ],

        ["M", ["pt.backWaistCenter.x","pt.backWaistCenter.y"] ],
        ["L", ["pt.backShoulderTip.x","pt.backShoulderTip.y"] ],

        ["M", ["pt.backAcrossCenter.x","pt.backAcrossCenter.y"] ],
        ["L", ["pt.backAcrossSide.x","pt.backAcrossSide.y"] ],
        ["M", ["pt.backHighbustCenter.x","pt.backHighbustCenter.y"] ],
        ["L", ["pt.backHighbustSide.x","pt.backHighbustSide.y"] ],
        ["M", ["pt.backBustCenter.x","pt.backBustCenter.y"] ],
        ["L", ["pt.backBustSide.x","pt.backBustSide.y"] ]

      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"1", "stroke":"#11FF11", "fill":"none" }
    },
    {
      "id": "Front Construction Lines",
      "type": "path",
      "d": [
        ["M", ["pt.frontNeckCenter.x","pt.frontNeckCenter.y"] ],
        ["L", ["pt.frontHipCenter.x","pt.frontHipCenter.y"] ],
        ["L", ["pt.frontHipSide.x","pt.frontHipSide.y"] ],
        ["L", ["pt.frontHighbustSide.x","pt.frontHighbustSide.y"] ],
        ["L", ["pt.frontAcrossSide.x","pt.frontAcrossSide.y"] ],
        ["L", ["pt.frontShoulderTip.x","pt.frontShoulderTip.y"] ],
        ["L", ["pt.frontNeckSide.x","pt.frontNeckSide.y"] ],
        ["L", ["pt.frontNeckCenter.x","pt.frontNeckCenter.y"] ],

        ["M", ["pt.frontAcrossCenter.x","pt.frontAcrossCenter.y"] ],
        ["L", ["pt.frontAcrossSide.x","pt.frontAcrossSide.y"] ],

        ["M",["pt.frontHighbustCenter.x","pt.frontHighbustCenter.y"] ],
        ["L", ["pt.frontHighbustSide.x","pt.frontHighbustSide.y"] ],

        ["M",["pt.frontBustCenter.x","pt.frontBustCenter.y"] ],
        ["L", ["pt.frontBustSide.x","pt.frontBustSide.y"] ],

        ["M",["pt.frontWaistCenter.x","pt.frontWaistCenter.y"] ],
        ["L", ["pt.frontWaistSide.x","pt.frontWaistSide.y"] ],

        ["M",["pt.frontHipCenter.x","pt.frontHipCenter.y"] ],
        ["L", ["pt.frontHipSide.x","pt.frontHipSide.y"] ]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
    },
    {
      "id": "Front Measurement Lines",
      "type": "path",
      "d": [
        ["M", ["pt.frontWaistCenter.x","pt.frontWaistCenter.y"] ],
        ["L", ["pt.frontNeckSide.x","pt.frontNeckSide.y"] ],

        ["M", ["pt.frontWaistCenter.x","pt.frontWaistCenter.y"] ],
        ["L", ["pt.frontShoulderTip.x","pt.frontShoulderTip.y"] ],

        ["M", ["pt.frontAcrossCenter.x","pt.frontAcrossCenter.y"] ],
        ["L", ["pt.frontAcrossSide.x","pt.frontAcrossSide.y"] ],
        ["M", ["pt.frontHighbustCenter.x","pt.frontHighbustCenter.y"] ],
        ["L", ["pt.frontHighbustSide.x","pt.frontHighbustSide.y"] ],
        ["M", ["pt.frontBustCenter.x","pt.frontBustCenter.y"] ],
        ["L", ["pt.frontBustSide.x","pt.frontBustSide.y"] ]

      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"1", "stroke":"#11FF11", "fill":"none" }
    },
    {
      "id": "Sleeve Construction Lines",
      "type": "path",
      "d": [
        ["M", ["pt.c8.x","pt.c8.y"] ],
        ["L", ["pt.c3.x","pt.c3.y"] ],
        ["M", ["pt.c4.x","pt.c4.y"] ],
        ["L", ["pt.c6.x","pt.c6.y"] ],
        ["M", ["pt.c4.x","pt.c4.y"] ],
        ["L", ["pt.c8.x","pt.c8.y"] ],
        ["L", ["pt.c6.x","pt.c6.y"] ],
        ["M", ["pt.c9.x","pt.c9.y"] ],
        ["L", ["pt.c10.x","pt.c10.y"] ]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
    }
  ],
  "points": {
      "CM" : "convertUnit(1, 'cm', window.measurementData.clientdata.units)",
      "IN" : "convertUnit(1, 'in', window.measurementData.clientdata.units)",
      "seamAllowance" : "(5/8)*pt.IN",

      "backNape": { "x": "0.5", "y": "0.5" },
      "backHighbustCenter" : "down(pt.backNape, 1*meas.back_highbust_height)",
      "backAcrossCenter" : "down(pt.backNape, 0.66*dist(pt.backNape, pt.backHighbustCenter))",
      "backBustCenter": "down(pt.backNape, 1*meas.back_bust_height)",
      "backWaistCenter": "down(pt.backNape, 1*meas.back_waist_length)",
      "backHipCenter": "down(pt.backWaistCenter, 1.4*meas.back_hip_height)",

      "backNeckSide" : "onCircleAtX(pt.backWaistCenter, 1*meas.back_neck_balance + 1*pt.CM, pt.backNape.x + 0.5*meas.neck_diameter, 'y1 < y2')",
      "backShoulderTip": "intersectCircles(pt.backWaistCenter, 1*meas.back_shoulder_balance + 1*pt.CM, pt.backNeckSide, 1*meas.shoulder_length, 'x1 > x2')",
      "backAcrossSide": "right(pt.backAcrossCenter, 0.5*meas.across_back)",
      "backHighbustSide": "right(pt.backHighbustCenter, 0.5*meas.back_bust_width)",
      "backBustSide": "down(pt.backHighbustSide, 1*meas.side_underarm_to_bust_height)",
      "backWaistSide": "down(pt.backHighbustSide, 1*meas.side_waist_length)",
      "backHipSide": {"x":"pt.backWaistSide.x", "y":"pt.backHipCenter.y"},

      "frontHighbustCenter": "right(pt.backHighbustSide, 2*pt.seamAllowance + 3*pt.IN + 0.6*meas.front_bust_width)",
      "frontNeckCenter": "up(pt.frontHighbustCenter, 1*meas.front_highbust_height)",
      "frontBustCenter" : "down(pt.frontNeckCenter, 1*meas.front_bust_height)",
      "frontAcrossCenter" : "down(pt.frontNeckCenter, 0.66*dist(pt.frontNeckCenter, pt.frontHighbustCenter))",
      "frontWaistCenter": "down(pt.frontNeckCenter, 1*meas.front_waist_length)",
      "frontHipCenter": {"x":"pt.frontNeckCenter.x", "y":"pt.backHipCenter.y"},

      "frontNeckSide":"onCircleAtX(pt.frontWaistCenter, 1*meas.front_neck_balance + 1*pt.CM, pt.frontNeckCenter.x - 0.5*meas.neck_diameter, 'y1 < y2')",
      "frontShoulderTip": "intersectCircles(pt.frontWaistCenter, 1*meas.front_shoulder_balance + 1*pt.CM, pt.frontNeckSide, 1*meas.shoulder_length, 'x1 < x2')",
      "frontAcrossSide": "left(pt.frontAcrossCenter, 0.5*meas.across_chest)",
      "frontHighbustSide": "left(pt.frontHighbustCenter, 0.5*meas.front_bust_width)",
      "frontBustSide": "left(pt.frontBustCenter, 0.5*meas.front_bust_width)",
      "frontWaistSide": "down(pt.frontHighbustSide, 1*meas.side_waist_length)",
      "frontHipSide": {"x":"pt.frontWaistSide.x", "y":"pt.frontHipCenter.y"},

      "a1": "up(pt.backNape, 0.5*(Math.abs(pt.backNape.y - pt.backNeckSide.y)))",
      "a2" : "copy(pt.backWaistCenter)",
      "a3" : "copy(pt.backHipCenter)",
      "a4" : "copy(pt.backBustCenter)",
      "a5" : "copy(pt.backAcrossCenter)",
      "temp_a6" : "right(pt.backBustSide, 0.07*meas.back_bust_width)",
      "a6" : "down(pt.temp_a6,1*pt.IN)",
      "a7" : {"x":"pt.a6.x", "y":"pt.backWaistSide.y"},
      "a8" : {"x":"pt.a6.x", "y":"pt.backHipSide.y" },
      "a9" : "onLineAtLength(pt.backNeckSide, pt.backShoulderTip, 0.1*meas.neck_diameter + 0.5*pt.IN)",
      "a10": "right(pt.backShoulderTip, 0.2*meas.armcap_height)",
      "a11": {"x":"pt.a10.x - 0.25*(pt.a10.x - pt.backAcrossSide.x)", "y":"pt.backAcrossCenter.y"},

      "a1in" : "right(pt.a1, dist(pt.a1, pt.a9)/2)",
      "a9out" : "polar(pt.a9, dist(pt.a1, pt.a9)/6, angleBetween(pt.a9, pt.a1in))",
      "a6out" : "left(pt.a6, dist(pt.a6, pt.a11)/3)",
      "a11in" : "polar(pt.a11, dist(pt.a6, pt.a11)/3, angleBetween(pt.a10, pt.a11))",

      "b1": "down(pt.frontNeckCenter, 0.1*meas.neck_diameter + 0.5*pt.IN)",
      "b2": "copy(pt.frontWaistCenter)",
      "b3": "copy(pt.frontHipCenter)",
      "b4": "copy(pt.frontBustCenter)",
      "b5": "copy(pt.frontAcrossCenter)",
      "temp_b6": "left(pt.frontBustSide, 0.07*meas.front_bust_width)",
      "b6" : {"x":"pt.temp_b6.x", "y":"pt.a6.y"},
      "b7": "down(pt.b6, dist(pt.a6, pt.a7))",
      "b8": "down(pt.b6, dist(pt.a6, pt.a8))",
      "b9": "onLineAtLength(pt.frontNeckSide, pt.frontShoulderTip, 0.1*meas.neck_diameter + 0.5*pt.IN)",
      "b10": "onCircleAtY(pt.b9, dist(pt.a9, pt.a10), pt.frontShoulderTip.y, 'x1 < x2')",
      "b11": {"x":"pt.b10.x - 0.25*(pt.b10.x - pt.frontAcrossSide.x)", "y":"pt.frontAcrossCenter.y"},

      "b1in" : {"x": "pt.b9.x", "y": "pt.b1.y"},
      "b9out" : "down(pt.b9, dist(pt.b1, pt.b9)/6)",
      "b6out" : "right(pt.b6, dist(pt.b6, pt.b11)/3)",
      "b11in" : "polar(pt.b11, dist(pt.b6, pt.b11)/3, angleBetween(pt.b10, pt.b11))",

      "c1": {"x":"pt.frontNeckCenter.x + 2*pt.seamAllowance + 4*pt.IN", "y":"pt.a10.y"},
      "c2": {"x":"pt.c1.x + Math.abs(pt.a6.x - pt.a10.x)", "y":"pt.c1.y"},
      "c3": "right(pt.c2, 0.4*meas.underarm_length)",
      "c4": "down(pt.c2, Math.abs(pt.a10.y - pt.a6.y))",
      "c5": {"x":"pt.c1.x - Math.abs(pt.a10.x - pt.a11.x)", "y":"pt.c1.y + Math.abs(pt.a10.y - pt.a11.y)"},
      "c6": "up(pt.c2, Math.abs(pt.b10.y - pt.b6.y))",
      "c7": {"x":"pt.c1.x - Math.abs(pt.b10.x - pt.b11.x)", "y":"pt.c2.y - Math.abs(pt.b10.y - pt.b11.y)"},
      "temp_c8x":"Math.min(pt.c5.x, pt.c7.x)",
      "c8": {"x":"pt.temp_c8x - Math.abs(pt.temp_c8x - pt.c1.x)", "y":"pt.c1.y"},
      "c9": "midPoint(pt.c4, pt.c6)",
      "c10": {"x":"pt.c3.x", "y":"pt.c9.y"},
      "c11": "down(pt.c10, 0.52*meas.bicep)",
      "c12": "up(pt.c10, 0.52*meas.bicep)",

      "c4out": "up(pt.c4, dist(pt.c4, pt.c5)/6)",
      "c8in" : "down(pt.c8, dist(pt.c5, pt.c8)/3)",
      "c8out" : "up(pt.c8, dist(pt.c8, pt.c7)/3)",
      "c6in": "down(pt.c6, dist(pt.c6, pt.c7)/6)",
      "c5in" : "polar(pt.c5, dist(pt.c4, pt.c5)/3, angleBetween(pt.c8in, pt.c4out))",
      "c5out" : "polar(pt.c5, dist(pt.c5, pt.c8)/3, angleBetween(pt.c4out, pt.c8in))",
      "c7in" : "polar(pt.c7, dist(pt.c8, pt.c7)/3, angleBetween(pt.c6in, pt.c8out))",
      "c7out" : "polar(pt.c7, dist(pt.c7, pt.c6)/3, angleBetween(pt.c8out, pt.c6in))",

      "d1" : "down(pt.c4, 2*pt.seamAllowance + 2*pt.IN)",
      "d2" : "down(pt.d1, 0.75*(2*curveLength(pt.a9, pt.a9out, pt.a1in, pt.a1)))",
      "d3" : "right(pt.d2, 1.5*pt.IN)",
      "d4" : { "x" : "pt.d3.x", "y" : "pt.d1.y" },

      "e1" : "right(pt.d4, 2*pt.seamAllowance + 2*pt.IN)",
      "e2" : "down(pt.e1, 0.75*(2*curveLength(pt.b9, pt.b9out, pt.b1in, pt.b1)))",
      "e3" : "right(pt.e2, 1.5*pt.IN)",
      "e4" : { "x" : "pt.e3.x", "y" : "pt.e1.y" },

      "f1": "right(pt.e4, 2*pt.seamAllowance + 2*pt.IN)",
      "f2": "down(pt.f1, dist(pt.a9, pt.a10))",
      "f3": "right(pt.f2, 0.25*pt.IN)",
      "f4": { "x" : "pt.f3.x", "y" : "pt.f1.y" }

  }
}
}
