        }
    ],

    "construction": [
        {
            "id": "Front Bodice Block Construction Lines",
            "type": "path",
            "d": [
                ["M", "pt.frontNeckCenter" ],
                ["L", "pt.frontWaistCenter", "pt.fWD_leg1", "pt.frontApex", "pt.fWD_leg2", "pt.frontWaistSide", "pt.frontHighbustSide" ],
                ["C", "pt.fHBS_out", "pt.fAS1_in", "pt.frontArmscye1", "pt.fAS1_out", "pt.fST_in", "pt.frontShoulderTip" ],
                ["L", "pt.frontNeckSide" ],
                ["C", "pt.fNS_out", "pt.fNC_in", "pt.frontNeckCenter"],
                ["M", "pt.frontWaistCenter"],
                ["L", "pt.t1_fWD_leg1", "pt.frontApex", "pt.t1_fWD_leg2", "pt.frontWaistSide" ],
                ["M", "pt.frontWaistCenter" ],
                ["L", "pt.t2_fWD_leg1", "pt.frontApex", "pt.t2_fWD_leg2", "pt.frontWaistSide" ],
                ["M", "pt.frontWaistCenter" ],
                ["L", "pt.t3_fWD_leg1", "pt.frontApex", "pt.t3_fWD_leg2", "pt.frontWaistSide" ]
            ],
            "drawattr": {},
            "appearanceattr": { "stroke-width":"1", "stroke":"#00FF00", "fill":"none" }
        },
        {
            "id": "Front Bodice Block Measurement Lines",
            "type": "path",
            "d": [
                ["M", "pt.frontNeckCore" ],
                ["L", "pt.frontNeckSide" ],
                ["M", "pt.frontNeckCenter" ],
                ["L", "pt.frontWaistCenter" ],
                ["M", "pt.frontAcrossCenter" ],
                ["L", "pt.frontAcrossSide" ],
                ["M", "pt.frontHighbustCenter" ],
                ["L", "pt.frontHighbustSide" ],
                ["M", "pt.frontBustCenter" ],
                ["L", "pt.frontBustSide" ],
                ["M", "pt.frontApex" ],
                ["L", "pt.t1_fBD_leg2" ],
                ["M", "pt.frontNeckSide" ],
                ["L", "pt.frontShoulderTip" ],
                ["M", "pt.frontWaistCenter" ],
                ["L", "pt.t1_fWD_leg1" ],
                ["M", "pt.t1_fWD_leg2" ],
                ["L", "pt.frontWaistSide" ],
                ["M", "pt.frontHighbustSide" ],
                ["L", "pt.frontWaistSide" ]
            ],
            "drawattr": {},
            "appearanceattr": { "stroke-width":"1", "stroke":"#FF0000", "fill":"none" }
        },
        {
            "id": "Front Bodice Block Balance Lines",
            "type": "path",
            "d": [
                ["M", "pt.frontWaistCenter" ],
                ["L", "pt.frontNeckSide" ],
                ["M", "pt.frontWaistCenter" ],
                ["L", "pt.frontShoulderTip" ],
                ["M", "pt.frontNeckCenter" ],
                ["L", "pt.frontWaistSide" ]
            ],
            "drawattr": {},
