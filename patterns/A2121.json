      "front_bust_height",
      "back_bust_width",
      "back_bust_height",

      "waist",
      "front_waist_length",
      "back_waist_length",
      "side_waist_length",

      "front_neck_balance",
      "back_neck_balance",
      "front_shoulder_balance",
      "back_shoulder_balance",

      "hip",
      "back_hip_height",

      "overarm_length",
      "underarm_length",
      "bicep",
      "armcap_height"
  ],
  "main": [
