  "points": {
    "CM": "convertUnit(1, 'cm', window.measurementData.clientdata.units)",
    "IN": "convertUnit(1, 'in', window.measurementData.clientdata.units)",
    "seamAllowance": "(5/8)*pt.IN",
    "patternOffset": "1*pt.IN",

    "frontNeckCenter": {"x":"3*pt.patternOffset + 1.1*meas.front_bust_width", "y":"pt.patternOffset + 1*meas.neck_diameter"},
    "frontWaistCenter": "down(pt.frontNeckCenter, 1*meas.front_waist_length)",
    "frontHibustCenter": "down(pt.frontNeckCenter, 1*meas.front_hibust_height)",
    "acrossChestCenter": "down(pt.frontNeckCenter, 0.5*dist(pt.frontNeckCenter, pt.frontHibustCenter))",
    "frontBustCenter": "down(pt.frontNeckCenter, 1*meas.front_bust_height)",

    "frontNeckSide": "onCircleAtX(pt.frontWaistCenter, 1*meas.front_neckheight_balance, pt.frontNeckCenter.x - 0.5*meas.neck_diameter, 'y1 < y2')",
    "frontShoulderTip":"intersectCircles(pt.frontNeckCenter,1*meas.front_shoulderwidth_balance,pt.frontNeckSide,1*meas.shoulder_length,'y1 > y2')",
    "acrossChestSide": "left(pt.acrossChestCenter, 0.5*meas.across_chest)",
    "frontHibustSide": "left(pt.frontHibustCenter, 0.5*meas.front_hibust_width)",
    "frontBustSide": "left(pt.frontBustCenter, 0.5*meas.front_bust_width)",
    "frontApex": "left(pt.frontBustCenter, 0.5*meas.bust_point_distance)",

    "frontArmscye1": {"x": "pt.frontShoulderTip.x + 0.8*Math.abs(pt.frontShoulderTip.x - pt.acrossChestSide.x)", "y": "pt.frontShoulderTip.y + 0.66*(pt.frontHibustCenter.y - pt.frontShoulderTip.y)"},
    "frontArmscye2": {"x": "pt.frontShoulderTip.x - 0.25*(pt.frontShoulderTip.x - pt.frontHibustSide.x)", "y": "pt.frontHibustCenter.y"},

    "temp_fWS": "onLineAtLength(pt.frontHibustSide, pt.frontBustSide, 1*meas.side_waist_length)",

    "temp_fWD1_leg1": {"x": "pt.frontApex.x", "y": "pt.frontWaistCenter.y"},
    "temp_fWD1_leg2": "intersectCircles(pt.frontApex, dist(pt.frontApex, pt.temp_fWD1_leg1), pt.temp_fWS, 0.5*meas.front_waist_width - dist(pt.frontWaistCenter, pt.temp_fWD1_leg1))",
    "front_dart_angle": "angleBetween3pts(pt.temp_fWD1_leg1, pt.frontApex, pt.temp_fWD1_leg2)",
    "fWD1_mid2": "polar(pt.frontApex, 1*pt.IN, angleBetween(pt.frontApex, pt.temp_fWD1_leg1) - 0.25*pt.front_dart_angle)",
    "fWD1_mid3": "polar(pt.frontApex, 1*pt.IN, angleBetween(pt.frontApex, pt.temp_fWD1_leg1) + 0.25*pt.front_dart_angle)",
    "fWD1_leg1": "intersectLines(pt.frontApex, pt.fWD1_mid2, pt.frontWaistCenter, pt.temp_fWD1_leg1)",
    "fWD1_leg2": "intersectLines(pt.frontApex, pt.fWD1_mid3, pt.frontWaistCenter, pt.temp_fWD1_leg1)",
    "fWD1_point": "down(pt.frontApex, dist(pt.frontApex, pt.fWD1_leg1)/7)",

    "fBD1_leg1": "copy(pt.frontBustSide)",
    "fBD1_leg2": "polar(pt.frontApex, dist(pt.frontApex, pt.fBD1_leg1), angleBetween(pt.frontApex, pt.fBD1_leg1) + 0.5*pt.front_dart_angle)",
    "fBD1_point": "polar(pt.frontApex, dist(pt.frontApex, pt.fBD1_leg1)/7, angleBetween(pt.frontApex, pt.fBD1_leg1) + 0.5*angleBetween3pts(pt.fBD1_leg1, pt.frontApex, pt.fBD1_leg2))",

    "frontWaistBalance_mid1": "intersectLines(pt.frontNeckCenter, pt.temp_fWS, pt.fBD1_point, pt.fBD1_leg2)",
    "frontWaistBalance_mid2": "onLineAtLength(pt.fBD1_point, pt.fBD1_leg1, dist(pt.fBD1_point, pt.frontWaistBalance_mid1))",
    "frontWaistSide": "intersectCircles(pt.fBD1_leg1, dist(pt.fBD1_leg2, pt.temp_fWS), pt.fWD1_leg2, 0.5*meas.front_waist_width - dist(pt.frontWaistCenter, pt.fWD1_leg1), 'y1 > y2')",

    "fNC_in": "left(pt.frontNeckCenter, 0.6*dist(pt.frontNeckSide, pt.frontNeckCenter))",
    "fNS_out": "down(pt.frontNeckSide, 0.15*dist(pt.frontNeckSide, pt.frontNeckCenter))",

    "fHBS_out": "polar(pt.frontHibustSide, dist(pt.frontHibustSide, pt.frontArmscye2)/3, angleBetween(pt.frontHibustSide, pt.fBD1_leg2) - radians(90))",
    "fAS2_in": "polar(pt.frontArmscye2, dist(pt.frontHibustSide, pt.frontArmscye2)/3, angleBetween(pt.frontArmscye1, pt.frontHibustSide))",

    "fAS2_out": "polar(pt.frontArmscye2, dist(pt.frontArmscye2, pt.frontArmscye1)/3, angleBetween(pt.frontHibustSide, pt.frontArmscye1))",
    "fAS1_in": "polar(pt.frontArmscye1, dist(pt.frontArmscye2, pt.frontArmscye1)/3, angleBetween(pt.frontShoulderTip, pt.frontArmscye2))",

    "fAS1_out": "polar(pt.frontArmscye1, dist(pt.frontArmscye1, pt.frontShoulderTip)/3, angleBetween(pt.frontArmscye2, pt.frontShoulderTip))",
    "fST_in": "polar(pt.frontShoulderTip, dist(pt.frontArmscye1, pt.frontShoulderTip)/6,angleBetween(pt.frontNeckSide, pt.frontShoulderTip) - radians(90))",

    "backHibustCenter": {"x":"pt.frontHibustSide.x - 2*pt.seamAllowance - 1*pt.patternOffset - 0.5*meas.front_bust_width", "y":"pt.frontHibustSide.y"},
    "backNeckCenter": "up(pt.backHibustCenter, 1*meas.back_hibust_height)",
    "backWaistCenter": "down(pt.backNeckCenter, 1*meas.back_waist_length)",
    "acrossBackCenter": "down(pt.backNeckCenter, 0.5*dist(pt.backNeckCenter, pt.backHibustCenter))",
    "backBustCenter": "down(pt.backNeckCenter, 1*meas.back_bust_height)",

    "backNeckSide": "onCircleAtX(pt.backWaistCenter, 1*meas.back_neckheight_balance, pt.backNeckCenter.x + 0.5*meas.neck_diameter, 'y1 < y2')",
    "temp_backShoulderTip":"intersectCircles(pt.backNeckCenter, 1*meas.back_shoulderwidth_balance, pt.backNeckSide, 1*meas.shoulder_length, 'y1 > y2')",
    "acrossBackSide": "right(pt.acrossBackCenter, 0.5*meas.across_back)",
    "backHibustSide": "right(pt.backHibustCenter, 0.5*meas.back_hibust_width)",
    "backBustSide": "right(pt.backBustCenter, 0.5*meas.back_bust_width)",
    "backApex1": {"x": "0.5*(pt.backHibustCenter.x + pt.acrossBackSide.x)", "y": "pt.backHibustCenter.y"},

    "backArmscye1": {"x": "pt.temp_backShoulderTip.x - 0.8*Math.abs(pt.temp_backShoulderTip.x - pt.acrossBackSide.x)", "y": "pt.temp_backShoulderTip.y + 0.66*(pt.backHibustCenter.y - pt.temp_backShoulderTip.y)"},
    "backArmscye2": {"x": "pt.acrossBackSide.x + 0.5*Math.abs(pt.acrossBackSide.x - pt.backHibustSide.x)", "y": "pt.backHibustCenter.y"},

    "back_dart_width": "(1*meas.back_waist_width)/12",
    "temp_bWS": "intersectCircles(pt.backHibustSide, 1*meas.side_waist_length, pt.backWaistCenter,  0.5*meas.back_waist_width + pt.back_dart_width, 'y1 > y2')",
    "temp_bWD1_leg1": "onLineAtX(pt.backWaistCenter, pt.temp_bWS, pt.backApex1.x)",
    "temp_bWD1_leg2": "intersectCircles(pt.backApex1, dist(pt.backApex1, pt.temp_bWD1_leg1), pt.temp_bWS, 0.5*meas.back_waist_width - dist(pt.backWaistCenter, pt.temp_bWD1_leg1), 'y1 > y2')",
    "back_dart_angle": "angleBetween3pts(pt.temp_bWD1_leg2, pt.backApex1, pt.temp_bWD1_leg1)",
    "temp_bWD1_mid1": "polar(pt.backApex1, 1*pt.IN, radians(90) + 0.5*(pt.back_dart_angle))",
    "temp_bWD1_mid2": "polar(pt.backApex1, 1*pt.IN, radians(90) - 0.5*(pt.back_dart_angle))",
    "bWD1_leg1": "intersectLines(pt.backWaistCenter, pt.temp_bWD1_leg1, pt.backApex1, pt.temp_bWD1_mid1)",
    "bWD1_leg2": "onLineAtLength(pt.backApex1, pt.temp_bWD1_mid2, dist(pt.backApex1, pt.bWD1_leg1))",
    "bWD1_point": "copy(pt.backApex1)",
    "backWaistSide": "intersectCircles(pt.backHibustSide, 1*meas.side_waist_length, pt.bWD1_leg2, 0.5*meas.back_waist_width - dist(pt.backWaistCenter, pt.bWD1_leg1), 'y1 > y2')",

    "backApex2": {"x": "pt.backApex1.x", "y": "pt.backNeckSide.y + (pt.acrossBackSide.y - pt.backNeckSide.y)/2"},
    "back_shoulder_dart_angle": "radians(7)",
    "bSD1_leg2": "onLineAtLength(pt.backNeckSide, pt.temp_backShoulderTip, dist(pt.backNeckSide, pt.temp_backShoulderTip)/6)",
    "bSD1_leg1": "polar(pt.backApex2, dist(pt.backApex2, pt.bSD1_leg2), angleBetween(pt.backApex2, pt.bSD1_leg2) + pt.back_shoulder_dart_angle)",
