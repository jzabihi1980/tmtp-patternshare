{
"pattern": {
  "title": "Bodice Block 2",
  "measurements": [

      "neck_diameter",

      "across_back",
      "across_chest",

      "shoulder_length",

      "front_highbust_width",
      "front_highbust_height",
      "back_highbust_width",
      "back_highbust_height",

      "bust",
      "front_bust_width",
      "front_bust_height",
      "back_bust_width",
      "back_bust_height",
      "bust_point_distance",

      "waist",
      "front_waist_width",
      "front_waist_length",
      "back_waist_width",
      "back_waist_length",
      "side_waist_length",

      "front_neck_balance",
      "back_neck_balance",
      "front_shoulder_balance",
      "back_shoulder_balance",
      "front_waist_balance",
      "back_waist_balance"

  ],

  "main": [
    {
      "id": "Front Bodice Block A",
      "type": "path",
      "d": [
        ["M", ["pt.frontNeckCenter.x","pt.frontNeckCenter.y"] ],
        ["L", ["pt.frontWaistCenter.x","pt.frontWaistCenter.y"] ],
        ["L", ["pt.fWD_leg1.x","pt.fWD_leg1.y"] ],
        ["L", ["pt.fWD_point.x","pt.fWD_point.y"] ],
        ["L", ["pt.fWD_leg2.x","pt.fWD_leg2.y"] ],
        ["L", ["pt.frontWaistSide.x","pt.frontWaistSide.y"] ],
        ["L", ["pt.fBD_leg1.x","pt.fBD_leg1.y"] ],
        ["L", ["pt.fBD_point.x","pt.fBD_point.y"] ],
        ["L", ["pt.fBD_leg2.x","pt.fBD_leg2.y"] ],
        ["L", ["pt.frontHighbustSide.x","pt.frontHighbustSide.y"] ],
        ["C", ["pt.fHBS_out.x","pt.fHBS_out.y"], ["pt.fAS2_in.x","pt.fAS2_in.y"],["pt.frontArmscye2.x","pt.frontArmscye2.y"] ],
        ["C", ["pt.fAS2_out.x","pt.fAS2_out.y"], ["pt.fAS1_in.x","pt.fAS1_in.y"], ["pt.frontArmscye1.x","pt.frontArmscye1.y"] ],
        ["C", ["pt.fAS1_out.x","pt.fAS1_out.y"], ["pt.fST_in.x","pt.fST_in.y"], ["pt.frontShoulderTip.x","pt.frontShoulderTip.y"] ],
        ["L", ["pt.frontNeckSide.x","pt.frontNeckSide.y"] ],
        ["C", ["pt.fNS_out.x","pt.fNS_out.y"], ["pt.fNC_in.x","pt.fNC_in.y"], ["pt.frontNeckCenter.x","pt.frontNeckCenter.y"] ],
        ["z"]
      ],
      "drawattr": {},
      "appearanceattr": {"stroke-width":"2", "stroke":"#111111", "fill":"none"}
    },
    {
      "id": "Back Bodice Block B",
      "type": "path",
      "d": [
        ["M", ["pt.backNeckCenter.x","pt.backNeckCenter.y"] ],
        ["L", ["pt.backWaistCenter.x","pt.backWaistCenter.y"] ],
        ["L", ["pt.bWD_leg1.x","pt.bWD_leg1.y"] ],
        ["L", ["pt.bWD_point.x","pt.bWD_point.y"] ],
        ["L", ["pt.bWD_leg2.x","pt.bWD_leg2.y"] ],
        ["L", ["pt.backWaistSide.x","pt.backWaistSide.y"] ],
        ["L", ["pt.backHighbustSide.x","pt.backHighbustSide.y"] ],
        ["C", ["pt.bHBS_out.x","pt.bHBS_out.y"], ["pt.bAS1_in.x","pt.bAS1_in.y"],["pt.backArmscye1.x","pt.backArmscye1.y"] ],
        ["C", ["pt.bAS1_out.x","pt.bAS1_out.y"], ["pt.bST_in.x","pt.bST_in.y"], ["pt.backShoulderTip.x","pt.backShoulderTip.y"] ],
        ["L", ["pt.bSD_leg1.x","pt.bSD_leg1.y"] ],
        ["L", ["pt.bSD_point.x","pt.bSD_point.y"] ],
        ["L", ["pt.bSD_leg2.x","pt.bSD_leg2.y"] ],
        ["L", ["pt.backNeckSide.x","pt.backNeckSide.y"] ],
        ["C", ["pt.bNS_out.x","pt.bNS_out.y"], ["pt.bNC_in.x","pt.bNC_in.y"], ["pt.backNeckCenter.x","pt.backNeckCenter.y"] ],
        ["z"]
      ],
      "drawattr": {},
      "appearanceattr": {"stroke-width":"2", "stroke":"#111111", "fill":"none"}
    }
  ],

  "construction": [
      {
      "id": "Front Bodice Block Lines",
      "type": "path",
      "d": [
        ["M", ["pt.frontNeckCenter.x","pt.frontNeckCenter.y"] ],
        ["L", ["pt.frontWaistCenter.x","pt.frontWaistCenter.y"] ],
        ["L", ["pt.fWD_leg1.x","pt.fWD_leg1.y"] ],
        ["L", ["pt.fWD_point.x","pt.fWD_point.y"] ],
        ["L", ["pt.fWD_leg2.x","pt.fWD_leg2.y"] ],
        ["L", ["pt.frontWaistSide.x","pt.frontWaistSide.y"] ],
        ["L", ["pt.fBD_leg1.x","pt.fBD_leg1.y"] ],
        ["L", ["pt.fBD_point.x","pt.fBD_point.y"] ],
        ["L", ["pt.fBD_leg2.x","pt.fBD_leg2.y"] ],
        ["L", ["pt.frontHighbustSide.x","pt.frontHighbustSide.y"] ],
        ["C", ["pt.fHBS_out.x","pt.fHBS_out.y"], ["pt.fAS2_in.x","pt.fAS2_in.y"],["pt.frontArmscye2.x","pt.frontArmscye2.y"] ],
        ["C", ["pt.fAS2_out.x","pt.fAS2_out.y"], ["pt.fAS1_in.x","pt.fAS1_in.y"], ["pt.frontArmscye1.x","pt.frontArmscye1.y"] ],
        ["C", ["pt.fAS1_out.x","pt.fAS1_out.y"], ["pt.fST_in.x","pt.fST_in.y"], ["pt.frontShoulderTip.x","pt.frontShoulderTip.y"] ],
        ["L", ["pt.frontNeckSide.x","pt.frontNeckSide.y"] ],
        ["C", ["pt.fNS_out.x","pt.fNS_out.y"], ["pt.fNC_in.x","pt.fNC_in.y"], ["pt.frontNeckCenter.x","pt.frontNeckCenter.y"] ]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
    },
      {
      "id": "Front Bodice Block Measurement Lines",
      "type": "path",
      "d": [
        ["M", ["pt.frontNeckCenter.x","pt.frontNeckCenter.y"] ],
        ["L", ["pt.frontWaistCenter.x","pt.frontWaistCenter.y"] ],
        ["M", ["pt.acrossChestCenter.x","pt.acrossChestCenter.y"] ],
        ["L", ["pt.acrossChestSide.x","pt.acrossChestSide.y"] ],
        ["M", ["pt.frontHighbustCenter.x","pt.frontHighbustCenter.y"] ],
        ["L", ["pt.frontHighbustSide.x","pt.frontHighbustSide.y"] ],
        ["M", ["pt.frontBustCenter.x","pt.frontBustCenter.y"] ],
        ["L", ["pt.frontBustSide.x","pt.frontBustSide.y"] ],

        ["M", ["pt.frontWaistCenter.x","pt.frontWaistCenter.y"] ],
        ["L", ["pt.frontNeckSide.x","pt.frontNeckSide.y"] ],
        ["M", ["pt.frontWaistCenter.x","pt.frontWaistCenter.y"] ],
        ["L", ["pt.frontShoulderTip.x","pt.frontShoulderTip.y"] ],

        ["M", ["pt.frontNeckCenter.x","pt.frontNeckCenter.y"] ],
        ["L", ["pt.t1_fWS.x","pt.t1_fWS.y"] ],


        ["M", ["pt.fBD_leg2.x", "pt.fBD_leg2.y"] ],
        ["L", ["pt.frontApex.x","pt.frontApex.y"] ],
        ["L", ["pt.fBD_leg1.x", "pt.fBD_leg1.y"] ],

        ["M", ["pt.p1.x", "pt.p1.y"] ],
        ["L", ["pt.frontWaistSide.x", "pt.frontWaistSide.y"] ],

        ["M", ["pt.fWD_leg2.x", "pt.fWD_leg2.y"] ],
        ["L", ["pt.frontApex.x","pt.frontApex.y"] ],
        ["L", ["pt.fWD_leg1.x", "pt.fWD_leg1.y"] ],

        ["M", ["pt.fBD_leg2.x", "pt.fBD_leg2.y"] ],
        ["L", ["pt.t1_fWS.x","pt.t1_fWS.y"] ],
        ["L", ["pt.t1_fWD_leg2.x","pt.t1_fWD_leg2.y"] ],
        ["L", ["pt.frontApex.x","pt.frontApex.y"] ],
        ["L", ["pt.t1_fWD_leg1.x","pt.t1_fWD_leg1.y"] ],
        ["L", ["pt.frontWaistCenter.x","pt.frontWaistCenter.y"] ]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"1", "stroke":"#11FF11", "fill":"none" }
    },
    {
      "id": "Back Bodice Block Lines",
      "type": "path",
      "d": [
        ["M", ["pt.backNeckCenter.x","pt.backNeckCenter.y"] ],
        ["L", ["pt.backWaistCenter.x","pt.backWaistCenter.y"] ],
        ["L", ["pt.bWD_leg1.x","pt.bWD_leg1.y"] ],
        ["L", ["pt.bWD_point.x","pt.bWD_point.y"] ],
        ["L", ["pt.bWD_leg2.x","pt.bWD_leg2.y"] ],
        ["L", ["pt.backWaistSide.x","pt.backWaistSide.y"] ],
        ["L", ["pt.backHighbustSide.x","pt.backHighbustSide.y"] ],
        ["C", ["pt.bHBS_out.x","pt.bHBS_out.y"], ["pt.bAS1_in.x","pt.bAS1_in.y"], ["pt.backArmscye1.x","pt.backArmscye1.y"] ],
        ["C", ["pt.bAS1_out.x","pt.bAS1_out.y"], ["pt.bST_in.x","pt.bST_in.y"], ["pt.backShoulderTip.x","pt.backShoulderTip.y"] ],
        ["L", ["pt.bSD_leg1.x","pt.bSD_leg1.y"] ],
        ["L", ["pt.bSD_point.x","pt.bSD_point.y"] ],
        ["L", ["pt.bSD_leg2.x","pt.bSD_leg2.y"] ],
        ["L", ["pt.backNeckSide.x","pt.backNeckSide.y"] ],
        ["L", ["pt.backNeckSide.x","pt.backNeckSide.y"] ],
        ["C", ["pt.bNS_out.x","pt.bNS_out.y"], ["pt.bNC_in.x","pt.bNC_in.y"], ["pt.backNeckCenter.x","pt.backNeckCenter.y"] ]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
    },
    {
      "id": "Back Bodice Block Measurement Lines",
      "type": "path",
      "d": [
        ["M", ["pt.backWaistCenter.x","pt.backWaistCenter.y"] ],
        ["L", ["pt.backNeckSide.x","pt.backNeckSide.y"] ],
        ["L", ["pt.temp1_backShoulderTip.x","pt.temp1_backShoulderTip.y"] ],
        ["L", ["pt.backWaistCenter.x","pt.backWaistCenter.y"] ],

        ["M", ["pt.bWD_mid4.x","pt.bWD_mid3.y"] ],
        ["L", ["pt.backShoulderTip.x","pt.backShoulderTip.y"] ],
        ["L", ["pt.backNeckSide.x","pt.backNeckSide.y"] ],

        ["M", ["pt.bSD_leg1.x","pt.bSD_leg1.y"] ],
        ["L", ["pt.backShoulderTip.x","pt.backShoulderTip.y"] ],

        ["M", ["pt.bSD_leg1.x","pt.bSD_leg1.y"] ],
        ["L", ["pt.backApex2.x","pt.backApex2.y"] ],
        ["L", ["pt.bSD_leg2.x","pt.bSD_leg2.y"] ],

        ["M", ["pt.acrossBackCenter.x","pt.acrossBackCenter.y"] ],
        ["L", ["pt.acrossBackSide.x","pt.acrossBackSide.y"] ],
        ["M", ["pt.backHighbustCenter.x","pt.backHighbustCenter.y"] ],
        ["L", ["pt.backHighbustSide.x","pt.backHighbustSide.y"] ],
        ["M", ["pt.backBustCenter.x","pt.backBustCenter.y"] ],
        ["L", ["pt.backBustSide.x","pt.backBustSide.y"] ],

        ["M", ["pt.bWD_leg1.x","pt.bWD_leg1.y"] ],
        ["L", ["pt.backApex1.x","pt.backApex1.y"] ],
        ["L", ["pt.bWD_leg2.x","pt.bWD_leg2.y"] ],

        ["M", ["pt.backHighbustSide.x","pt.backHighbustSide.y"] ],
        ["L", ["pt.temp_bWS.x","pt.temp_bWS.y"] ],
        ["L", ["pt.temp_bWD_leg2.x","pt.temp_bWD_leg2.y"] ],
        ["L", ["pt.backApex1.x","pt.backApex1.y"] ],
        ["L", ["pt.temp_bWD_leg1.x","pt.temp_bWD_leg1.y"] ],
        ["L", ["pt.backWaistCenter.x","pt.backWaistCenter.y"] ]
      ],
      "drawattr": {},
      "appearanceattr": { "stroke-width":"1", "stroke":"#11FF11", "fill":"none" }
    }
  ],

  "points": {
    "CM": "convertUnit(1, 'cm', window.measurementData.clientdata.units)",
    "IN": "convertUnit(1, 'in', window.measurementData.clientdata.units)",
    "seamAllowance": "(5/8)*pt.IN",
    "patternOffset": "1*pt.IN",

    "frontNeckCenter": {"x":"3*pt.patternOffset + 1.1*meas.front_bust_width", "y":"pt.patternOffset + 1*meas.neck_diameter"},
    "frontWaistCenter": "down(pt.frontNeckCenter, 1*meas.front_waist_length)",
    "frontHighbustCenter": "down(pt.frontNeckCenter, 1*meas.front_highbust_height)",
    "acrossChestCenter": "down(pt.frontNeckCenter, 0.5*dist(pt.frontNeckCenter, pt.frontHighbustCenter))",
    "frontBustCenter": "down(pt.frontNeckCenter, 1*meas.front_bust_height)",

    "frontNeckSide": "onCircleAtX(pt.frontWaistCenter, 1*meas.front_neck_balance, pt.frontNeckCenter.x - 0.5*meas.neck_diameter, 'y1 < y2')",
    "frontShoulderTip":"intersectCircles(pt.frontWaistCenter, 1*meas.front_shoulder_balance, pt.frontNeckSide, 1*meas.shoulder_length,'y1 > y2')",
    "acrossChestSide": "left(pt.acrossChestCenter, 0.5*meas.across_chest)",
    "frontHighbustSide": "left(pt.frontHighbustCenter, 0.5*meas.front_highbust_width)",
    "frontBustSide": "left(pt.frontBustCenter, 0.5*meas.front_bust_width)",
    "frontApex": "left(pt.frontBustCenter, 0.5*meas.bust_point_distance)",

    "frontArmscye1": {"x": "pt.frontShoulderTip.x + 0.8*Math.abs(pt.frontShoulderTip.x - pt.acrossChestSide.x)", "y": "pt.frontShoulderTip.y + 0.66*(pt.frontHighbustCenter.y - pt.frontShoulderTip.y)"},
    "frontArmscye2": {"x": "pt.frontShoulderTip.x - 0.25*(pt.frontShoulderTip.x - pt.frontHighbustSide.x)", "y": "pt.frontHighbustCenter.y"},

    "fBD_height": "Math.abs(Math.sqrt(Math.pow(dist(pt.frontApex, pt.frontHighbustSide), 2) - Math.pow(dist(pt.frontApex, pt.frontBustSide),2)))",
    "fBD_leg2": "intersectCircles(pt.frontHighbustSide, pt.fBD_height, pt.frontApex, dist(pt.frontApex, pt.frontBustSide), 'y1 > y2')",

    "t1_fWS": "intersectCircles(pt.frontNeckCenter, 1*meas.front_waist_balance, pt.fBD_leg2, 1*meas.side_waist_length - dist(pt.frontHighbustSide, pt.fBD_leg2), 'y1 > y2')",

    "t1_fWD_leg1": {"x": "pt.frontApex.x", "y": "pt.frontWaistCenter.y"},
    "t1_fWD_leg2": "intersectCircles(pt.frontApex, dist(pt.frontApex, pt.t1_fWD_leg1), pt.t1_fWS, 0.5*meas.front_waist_width - dist(pt.frontWaistCenter, pt.t1_fWD_leg1))",

    "total_dart_width": "angleBetween3pts(pt.t1_fWD_leg1, pt.frontApex, pt.t1_fWD_leg2)",

    "fBD_leg1": "polar(pt.frontApex, dist(pt.frontApex, pt.fBD_leg2), angleBetween(pt.frontApex, pt.fBD_leg2) - 0.5*pt.total_dart_width)",
    "fBD_point": "polar(pt.frontApex, dist(pt.frontApex, pt.fBD_leg1)/7, angleBetween(pt.frontApex, pt.fBD_leg1) + 0.5*angleBetween3pts(pt.fBD_leg1, pt.frontApex, pt.fBD_leg2))",

    "p2": "intersectLines(pt.frontNeckCenter, pt.t1_fWS, pt.frontApex, pt.fBD_leg2)",
    "p1": "onLineAtX(pt.frontApex, pt.fBD_leg1, pt.p2.x)",
    "frontWaistSide": "intersectCircles(pt.fBD_leg1, dist(pt.fBD_leg2, pt.t1_fWS), pt.p1, dist(pt.p2, pt.t1_fWS), 'y1 > y2')",

    "t2_fWD_leg2": "intersectCircles(pt.frontApex, dist(pt.frontApex, pt.t1_fWD_leg1), pt.frontWaistSide, 0.5*meas.front_waist_width - dist(pt.frontWaistCenter, pt.t1_fWD_leg1), 'y1 > y2')",

    "front_waist_dart_angle": "angleBetween3pts(pt.t1_fWD_leg1, pt.frontApex, pt.t2_fWD_leg2)",

    "p3": "polar(pt.frontApex, 1*pt.IN, radians(90) - 0.5*pt.front_waist_dart_angle)",
    "fWD_leg1": "onLineAtY(pt.frontApex, pt.p3, pt.frontWaistCenter.y)",
    "fWD_leg2": "onLineAtLength(pt.t2_fWD_leg2, pt.frontWaistSide, -dist(pt.t1_fWD_leg1, pt.fWD_leg1))",
    "fWD_point": "down(pt.frontApex, dist(pt.frontApex, pt.fWD_leg1)/7)",

    "pattern_waist": {"x": "pt.frontWaistCenter.x - dist(pt.frontWaistCenter, pt.fWD_leg1) - dist(pt.fWD_leg2, pt.frontWaistSide)", "y": "pt.frontWaistCenter.y + 2*pt.IN"},
    "measured_waist": {"x": "pt.frontWaistCenter.x - 0.5*meas.front_waist_width", "y": "pt.frontWaistCenter.y + 3*pt.IN"},



    "fNC_in": "left(pt.frontNeckCenter, 0.6*dist(pt.frontNeckSide, pt.frontNeckCenter))",
    "fNS_out": "down(pt.frontNeckSide, 0.15*dist(pt.frontNeckSide, pt.frontNeckCenter))",

    "fHBS_out": "polar(pt.frontHighbustSide, dist(pt.frontHighbustSide, pt.frontArmscye2)/3, angleBetween(pt.frontHighbustSide, pt.fBD_leg2) - radians(90))",
    "fAS2_in": "polar(pt.frontArmscye2, dist(pt.frontHighbustSide, pt.frontArmscye2)/3, angleBetween(pt.frontArmscye1, pt.frontHighbustSide))",

    "fAS2_out": "polar(pt.frontArmscye2, dist(pt.frontArmscye2, pt.frontArmscye1)/3, angleBetween(pt.frontHighbustSide, pt.frontArmscye1))",
    "fAS1_in": "polar(pt.frontArmscye1, dist(pt.frontArmscye2, pt.frontArmscye1)/3, angleBetween(pt.frontShoulderTip, pt.frontArmscye2))",

    "fAS1_out": "polar(pt.frontArmscye1, dist(pt.frontArmscye1, pt.frontShoulderTip)/3, angleBetween(pt.frontArmscye2, pt.frontShoulderTip))",
    "fST_in": "polar(pt.frontShoulderTip, dist(pt.frontArmscye1, pt.frontShoulderTip)/6,angleBetween(pt.frontNeckSide, pt.frontShoulderTip) - radians(90))",

    "backHighbustCenter": {"x":"pt.t1_fWS.x - 2*pt.seamAllowance - 1*pt.patternOffset - 0.5*meas.front_bust_width", "y":"pt.frontHighbustSide.y"},
    "backNeckCenter": "up(pt.backHighbustCenter, 1*meas.back_highbust_height)",
    "backWaistCenter": "down(pt.backNeckCenter, 1*meas.back_waist_length)",
    "acrossBackCenter": "down(pt.backNeckCenter, 0.5*dist(pt.backNeckCenter, pt.backHighbustCenter))",
    "backBustCenter": "down(pt.backNeckCenter, 1*meas.back_bust_height)",

    "backNeckSide": "onCircleAtX(pt.backWaistCenter, 1*meas.back_neck_balance, pt.backNeckCenter.x + 0.5*meas.neck_diameter, 'y1 < y2')",
    "temp1_backShoulderTip":"intersectCircles(pt.backWaistCenter, 1*meas.back_shoulder_balance, pt.backNeckSide, 1*meas.shoulder_length, 'y1 > y2')",
    "acrossBackSide": "right(pt.acrossBackCenter, 0.5*meas.across_back)",
    "backHighbustSide": "right(pt.backHighbustCenter, 0.5*meas.back_highbust_width)",
    "backBustSide": "right(pt.backBustCenter, 0.5*meas.back_bust_width)",
    "backApex1": {"x": "0.5*(pt.backHighbustCenter.x + pt.acrossBackSide.x)", "y": "pt.backHighbustCenter.y"},

    "backArmscye1": {"x": "pt.temp1_backShoulderTip.x - 0.8*Math.abs(pt.temp1_backShoulderTip.x - pt.acrossBackSide.x)", "y": "pt.temp1_backShoulderTip.y + 0.66*(pt.backHighbustCenter.y - pt.temp1_backShoulderTip.y)"},
    "backArmscye2": {"x": "pt.acrossBackSide.x + 0.5*Math.abs(pt.acrossBackSide.x - pt.backHighbustSide.x)", "y": "pt.backHighbustCenter.y"},

    "back_dart_width": "(1*meas.back_waist_width)/12",
    "temp_bWS": "intersectCircles(pt.backHighbustSide, 1*meas.side_waist_length, pt.backWaistCenter,  0.5*meas.back_waist_width + pt.back_dart_width, 'y1 > y2')",
    "temp_bWD_leg1": "onLineAtX(pt.backWaistCenter, pt.temp_bWS, pt.backApex1.x)",
    "temp_bWD_leg2": "intersectCircles(pt.backApex1, dist(pt.backApex1, pt.temp_bWD_leg1), pt.temp_bWS, 0.5*meas.back_waist_width - dist(pt.backWaistCenter, pt.temp_bWD_leg1), 'y1 > y2')",
    "back_dart_angle": "angleBetween3pts(pt.temp_bWD_leg2, pt.backApex1, pt.temp_bWD_leg1)",
    "bWD_mid1": "polar(pt.backApex1, 1*pt.IN, radians(90) + 0.5*(pt.back_dart_angle))",
    "bWD_mid2": "polar(pt.backApex1, 1*pt.IN, radians(90) - 0.5*(pt.back_dart_angle))",
    "bWD_leg1": "intersectLines(pt.backWaistCenter, pt.temp_bWD_leg1, pt.backApex1, pt.bWD_mid1)",
    "bWD_leg2": "onLineAtLength(pt.backApex1, pt.bWD_mid2, dist(pt.backApex1, pt.bWD_leg1))",
    "bWD_point": "copy(pt.backApex1)",
    "backWaistSide": "intersectCircles(pt.backHighbustSide, 1*meas.side_waist_length, pt.bWD_leg2, 0.5*meas.back_waist_width - dist(pt.backWaistCenter, pt.bWD_leg1), 'y1 > y2')",

    "bWD_mid3": "intersectLines(pt.backWaistCenter, pt.temp1_backShoulderTip, pt.backApex1, pt.bWD_leg1)",
    "bWD_mid4": "onLineAtLength(pt.backApex1, pt.bWD_leg2, dist(pt.backApex1, pt.bWD_mid3))",
    "backShoulderTip": "right(pt.temp1_backShoulderTip, dist(pt.bWD_mid3, pt.bWD_mid4))",

    "back_shoulder_dart_angle": "radians(7)",
    "bSD_leg2": "onLineAtLength(pt.backNeckSide, pt.backShoulderTip, dist(pt.backNeckSide, pt.backShoulderTip)/6)",
    "bSD_leg1": "onLineAtLength(pt.bSD_leg2, pt.backShoulderTip, Math.abs(1*meas.shoulder_length - dist(pt.backNeckSide, pt.backShoulderTip)))",
    "bSD_mid": "midPoint(pt.bSD_leg1, pt.bSD_leg2)",
    "backApex2": "polar(pt.bSD_mid, 0.5*dist(pt.backNeckSide, pt.acrossBackCenter), angleBetween(pt.backNeckSide, pt.bSD_mid) + radians(90))",
    "bSD_point": "polar(pt.backApex2, dist(pt.backApex2, pt.bSD_mid)/5, angleBetween(pt.backApex2, pt.bSD_mid))",

    "bNC_in": "right(pt.backNeckCenter, 0.6*dist(pt.backNeckSide, pt.backNeckCenter))",
    "bNS_out": "polar(pt.backNeckSide, 0.15*dist(pt.backNeckSide, pt.backNeckCenter), angleBetween(pt.backNeckSide, pt.bNC_in))",

    "bHBS_out": "polar(pt.backHighbustSide, dist(pt.backHighbustSide, pt.backArmscye1)/2, angleBetween(pt.backHighbustSide, pt.backWaistSide) + radians(90))",
    "bAS1_in": "polar(pt.backArmscye1, dist(pt.backHighbustSide, pt.backArmscye1)/3, angleBetween(pt.backShoulderTip, pt.backHighbustSide))",

    "bAS1_out": "polar(pt.backArmscye1, dist(pt.backArmscye1, pt.backShoulderTip)/3, angleBetween(pt.backHighbustSide, pt.backShoulderTip))",
    "bST_in": "polar(pt.backShoulderTip, dist(pt.backArmscye1, pt.backShoulderTip)/6, angleBetween(pt.backNeckSide, pt.backShoulderTip) + radians(90))"

  }

}
