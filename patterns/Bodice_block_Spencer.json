        "points": [
            {
                "type": "formula",
                "name": "CM",
                "formula": "convertUnit(1, 'cm', window.measurementData.clientdata.units)"
            },
            {
                "type": "formula",
                "name": "IN",
                "formula": "convertUnit(1, 'in', window.measurementData.clientdata.units)"
            },
            {
                "type": "formula",
                "name": "seamAllowance",
                "formula": "(5/8)*pt.IN"
            },
            {
                "type": "formula",
                "name": "patternOffset",
                "formula": "1*pt.IN"
            },
            {
                "type": "point_xy",
                "name": "frontNeckCenter",
                "x": "3*pt.patternOffset + 1.1*meas.front_bust_width",
                "y": "pt.patternOffset + 1*meas.neck_diameter"
            },
            {
                "type": "formula",
                "name": "frontWaistCenter",
                "formula": "down(pt.frontNeckCenter, 1*meas.front_waist_length)"
            },
            {
                "type": "formula",
                "name": "frontHighbustCenter",
                "formula": "down(pt.frontNeckCenter, 1*meas.front_highbust_height)"
            },
            {
                "type": "formula",
                "name": "frontAcrossCenter",
                "formula": "down(pt.frontNeckCenter, 0.6*dist(pt.frontNeckCenter, pt.frontHighbustCenter))"
            },
            {
                "type": "formula",
                "name": "frontBustCenter",
                "formula": "down(pt.frontNeckCenter, 1*meas.front_bust_height)"
            },
            {
                "type": "object",
                "name": "frontNeckSide",
                "object": "onCircleAtX(pt.frontWaistCenter, 1*meas.front_neck_balance, pt.frontNeckCenter.x - 0.5*meas.neck_diameter, 'y1 < y2')"
            },
            {
                "type": "point_xy",
                "name": "frontNeckCore",
                "x": "pt.frontNeckCenter.x",
                "y": "pt.frontNeckSide.y"
            },
            {
                "type": "object",
                "name": "frontShoulderTip",
                "object": "intersectCircles(pt.frontWaistCenter, 1*meas.front_shoulder_balance, pt.frontNeckSide, 1*meas.shoulder_length,'y1 > y2')"
            },
            {
                "type": "formula",
                "name": "frontAcrossSide",
                "formula": "left(pt.frontAcrossCenter, 0.5*meas.across_chest)"
            },
            {
                "type": "formula",
                "name": "frontHighbustSide",
                "formula": "left(pt.frontHighbustCenter, 0.5*meas.front_highbust_width)"
            },
            {
                "type": "formula",
                "name": "frontBustSide",
                "formula": "left(pt.frontBustCenter, 0.5*meas.front_bust_width)"
            },
            {
                "type": "formula",
                "name": "frontApex",
                "formula": "left(pt.frontBustCenter, 0.5*meas.bust_point_distance)"
            },
            {
                "type": "point_xy",
                "name": "frontArmscye1",
                "x": "pt.frontShoulderTip.x + 0.8*Math.abs(pt.frontShoulderTip.x - pt.frontAcrossSide.x)",
                "y": "pt.frontAcrossSide.y"
            },
            {
                "type": "point_xy",
                "name": "frontArmscye2",
                "x": "pt.frontShoulderTip.x - 0.25*(pt.frontShoulderTip.x - pt.frontHighbustSide.x)",
                "y": "pt.frontHighbustCenter.y"
            },
            {
                "type": "formula",
                "name": "fBD_height",
                "formula": "Math.abs(Math.sqrt(Math.pow(dist(pt.frontApex, pt.frontHighbustSide), 2) - Math.pow(dist(pt.frontApex, pt.frontBustSide),2)))"
            },
            {
                "type": "object",
                "name": "t1_fBD_leg2",
                "object": "intersectCircles(pt.frontHighbustSide, pt.fBD_height, pt.frontApex, dist(pt.frontApex, pt.frontBustSide), 'y1 > y2')"
            },
            {
                "type": "object",
                "name": "t1_fWS",
                "object": "intersectCircles(pt.frontNeckCenter, 1*meas.front_waist_balance, pt.t1_fBD_leg2, 1*meas.side_waist_length - dist(pt.frontHighbustSide, pt.t1_fBD_leg2), 'y1 > y2')"
            },
            {
                "type": "point_xy",
                "name": "t1_fWD_leg1",
                "x": "pt.frontApex.x",
                "y": "pt.frontWaistCenter.y"
            },
            {
                "type": "object",
                "name": "t1_fWD_leg2",
                "object": "intersectCircles(pt.frontApex, dist(pt.frontApex, pt.t1_fWD_leg1), pt.t1_fWS, 0.5*meas.front_waist_width - dist(pt.frontWaistCenter, pt.t1_fWD_leg1), 'y1 > y2')"
            },
            {
                "type": "formula",
                "name": "t1_fWD_angle",
                "formula": "angleBetween3pts(pt.t1_fWD_leg1, pt.frontApex, pt.t1_fWD_leg2)"
            },
            {
                "type": "object",
                "name": "fBD_leg2",
                "object": "intersectVectorLine(pt.frontApex, radians(180) + 0.25*pt.t1_fWD_angle, pt.frontHighbustSide, pt.t1_fWS)"
            },
            {
                "type": "object",
                "name": "fBD_leg1",
                "object": "rotate(pt.fBD_leg2, pt.frontApex, -0.5*pt.t1_fWD_angle)"
            },
            {
                "type": "object",
                "name": "fBD_point",
                "object": "polar(pt.frontApex, dist(pt.frontApex, pt.fBD_leg1)/7, angleBetween(pt.frontApex, pt.fBD_leg1) + 0.5*angleBetween3pts(pt.fBD_leg1, pt.frontApex, pt.fBD_leg2))"
            },
            {
                "type": "object",
                "name": "p2",
                "object": "intersectLines(pt.frontNeckCenter, pt.t1_fWS, pt.frontApex, pt.fBD_leg2)"
            },
            {
                "type": "object",
                "name": "p1",
                "object": "onLineAtX(pt.frontApex, pt.fBD_leg1, pt.p2.x)"
            },
            {
                "type": "object",
                "name": "frontWaistSide",
                "object": "intersectCircles(pt.fBD_leg1, dist(pt.fBD_leg2, pt.t1_fWS), pt.p1, dist(pt.p2, pt.t1_fWS), 'y1 > y2')"
            },
            {
                "type": "formula",
                "name": "rotate_waist_angle",
                "formula": "angleBetween3pts(pt.frontWaistSide, pt.frontApex, pt.t1_fWS)"
            },
            {
                "type": "object",
                "name": "t2_fWD_leg2",
                "object": "rotate(pt.t1_fWD_leg2, pt.frontApex, -pt.rotate_waist_angle)"
            },
            {
                "type": "formula",
                "name": "t2_fWD_angle",
                "formula": "angleBetween3pts(pt.t1_fWD_leg1, pt.frontApex, pt.t2_fWD_leg2)"
            },
            {
                "type": "object",
                "name": "t3_fWD_leg1",
                "object": "intersectVectorLine(pt.frontApex, radians(90) - 0.5*pt.t2_fWD_angle, pt.frontWaistCenter, pt.t1_fWD_leg1)"
            },
            {
                "type": "object",
                "name": "t3_fWD_leg2",
                "object": "polar(pt.frontApex, dist(pt.frontApex, pt.t3_fWD_leg1), radians(90) + 0.5*pt.t2_fWD_angle)"
            },
            {
                "type": "formula",
                "name": "t3_fWD_angle",
                "formula": "angleBetween3pts(pt.t3_fWD_leg1, pt.frontApex, pt.t3_fWD_leg2)"
            },
            {
                "type": "formula",
                "name": "fWD_extension_angle",
                "formula": "radians(90) - angleBetween3pts(pt.t3_fWD_leg2, pt.t3_fWD_leg1, pt.frontApex)"
            },
            {
                "type": "formula",
                "name": "fWD_extension_opposite_height",
                "formula": "Math.sin(pt.fWD_extension_angle) * dist(pt.frontWaistCenter, pt.t3_fWD_leg1)"
            },
            {
                "type": "object",
                "name": "t4_fWD_leg1",
                "object": "onLineAtLength(pt.t3_fWD_leg1, pt.frontApex, -pt.fWD_extension_opposite_height)"
            },
            {
                "type": "object",
                "name": "t4_fWD_leg2",
                "object": "onLineAtLength(pt.t3_fWD_leg2, pt.frontApex, -pt.fWD_extension_opposite_height)"
            },
            {
                "type": "formula",
                "name": "front_waist_delta",
                "formula": "0.5*meas.front_waist_width - (dist(pt.frontWaistCenter, pt.t4_fWD_leg1) + dist(pt.t4_fWD_leg2, pt.frontWaistSide))"
            },
            {
                "type": "object",
                "name": "fWD_leg1",
                "object": "onLineAtLength(pt.t4_fWD_leg1, pt.frontWaistCenter, -0.5*pt.front_waist_delta)"
            },
            {
                "type": "object",
                "name": "fWD_leg2",
                "object": "onLineAtLength(pt.t4_fWD_leg2, pt.frontWaistSide, -0.5*pt.front_waist_delta)"
            },
            {
                "type": "formula",
                "name": "fWD_point",
                "formula": "down(pt.frontApex, dist(pt.frontApex, pt.fWD_leg1)/7)"
            },
            {
                "type": "point_xy",
                "name": "front_pattern_waist",
                "x": "pt.frontWaistCenter.x - (dist(pt.frontWaistCenter, pt.fWD_leg1) + dist(pt.fWD_leg2, pt.frontWaistSide))",
                "y": "pt.frontWaistCenter.y + 2*pt.IN"
            },
            {
                "type": "point_xy",
                "name": "front_measured_waist",
                "x": "pt.frontWaistCenter.x - 0.5*meas.front_waist_width",
                "y": "pt.frontWaistCenter.y + 3*pt.IN"
            },
            {
                "type": "formula",
                "name": "fNC_in",
                "formula": "left(pt.frontNeckCenter, 0.6*dist(pt.frontNeckSide, pt.frontNeckCenter))"
            },
            {
                "type": "formula",
                "name": "fNS_out",
                "formula": "down(pt.frontNeckSide, 0.15*dist(pt.frontNeckSide, pt.frontNeckCenter))"
            },
            {
                "type": "object",
                "name": "fHBS_out",
                "object": "polar(pt.frontHighbustSide, dist(pt.frontHighbustSide, pt.frontArmscye1)/2, angleBetween(pt.frontHighbustSide, pt.fBD_leg2) - radians(90))"
            },
            {
                "type": "object",
                "name": "fAS1_in",
                "object": "polar(pt.frontArmscye1, dist(pt.frontHighbustSide, pt.frontArmscye1)/3, angleBetween(pt.frontShoulderTip, pt.frontArmscye2))"
            },
            {
                "type": "object",
                "name": "fAS1_out",
                "object": "polar(pt.frontArmscye1, dist(pt.frontArmscye1, pt.frontShoulderTip)/3, angleBetween(pt.fAS1_in, pt.frontArmscye1))"
            },
            {
                "type": "object",
                "name": "fST_in",
                "object": "polar(pt.frontShoulderTip, dist(pt.frontArmscye1, pt.frontShoulderTip)/6,angleBetween(pt.frontNeckSide, pt.frontShoulderTip) - radians(90))"
            },
            {
                "type": "point_xy",
                "name": "backHighbustCenter",
                "x": "pt.t1_fWS.x - 2*pt.seamAllowance - 1*pt.patternOffset - 0.5*meas.front_bust_width",
                "y": "pt.frontHighbustSide.y"
            },
            {
                "type": "formula",
                "name": "backNeckCenter",
                "formula": "up(pt.backHighbustCenter, 1*meas.back_highbust_height)"
            },
            {
                "type": "formula",
                "name": "backAcrossCenter",
                "formula": "down(pt.backNeckCenter, 0.66*dist(pt.backNeckCenter, pt.backHighbustCenter))"
            },
            {
                "type": "formula",
                "name": "backWaistCenter",
                "formula": "down(pt.backNeckCenter, 1*meas.back_waist_length)"
            },
            {
                "type": "formula",
                "name": "backBustCenter",
                "formula": "down(pt.backNeckCenter, 1*meas.back_bust_height)"
            },
            {
                "type": "object",
                "name": "backNeckSide",
                "object": "onCircleAtX(pt.backWaistCenter, 1*meas.back_neck_balance, pt.backNeckCenter.x + 0.5*meas.neck_diameter, 'y1 < y2')"
            },
            {
                "type": "point_xy",
                "name": "backNeckCore",
                "x": "pt.backNeckCenter.x",
                "y": "pt.backNeckSide.y"
            },
            {
                "type": "object",
                "name": "t1_bST",
                "object": "intersectCircles(pt.backWaistCenter, 1*meas.back_shoulder_balance, pt.backNeckSide, 1*meas.shoulder_length, 'y1 > y2')"
            },
            {
                "type": "formula",
                "name": "backAcrossSide",
                "formula": "right(pt.backAcrossCenter, 0.5*meas.across_back)"
            },
            {
                "type": "formula",
                "name": "backHighbustSide",
                "formula": "right(pt.backHighbustCenter, 0.5*meas.back_highbust_width)"
            },
            {
                "type": "formula",
                "name": "backBustSide",
                "formula": "right(pt.backBustCenter, 0.5*meas.back_bust_width)"
            },
            {
                "type": "point_xy",
                "name": "backApex1",
                "x": "0.5*(pt.backHighbustCenter.x + pt.backAcrossSide.x)",
                "y": "pt.backHighbustCenter.y"
            },
            {
                "type": "formula",
                "name": "back_dart_width",
                "formula": "(1*meas.back_waist_width)/12"
            },
            {
                "type": "object",
                "name": "t1_bWS",
                "object": "intersectCircles(pt.backHighbustSide, 1*meas.side_waist_length, pt.backWaistCenter,  0.5*meas.back_waist_width + pt.back_dart_width, 'y1 > y2')"
            },
            {
                "type": "point_xy",
                "name": "t1_bWD_leg1",
                "x": "pt.backApex1.x",
                "y": "pt.backWaistCenter.y"
            },
            {
                "type": "object",
                "name": "t1_bWD_leg2",
                "object": "intersectCircles(pt.backApex1, dist(pt.backApex1, pt.t1_bWD_leg1), pt.t1_bWS, 0.5*meas.back_waist_width - dist(pt.backWaistCenter, pt.t1_bWD_leg1), 'y1 > y2')"
            },
            {
                "type": "formula",
                "name": "back_dart_angle",
                "formula": "angleBetween3pts(pt.t1_bWD_leg2, pt.backApex1, pt.t1_bWD_leg1)"
            },
            {
                "type": "object",
                "name": "t2_bWD_leg1",
                "object": "intersectVectorLine(pt.backApex1, radians(90) + 0.5*(pt.back_dart_angle), pt.backWaistCenter, pt.t1_bWD_leg1)"
            },
            {
                "type": "object",
                "name": "t2_bWD_leg2",
                "object": "intersectVectorLine(pt.backApex1, radians(90) - 0.5*(pt.back_dart_angle), pt.backWaistCenter, pt.t1_bWD_leg1)"
            },
            {
                "type": "formula",
                "name": "bWD_extension_angle",
                "formula": "radians(90) - angleBetween3pts(pt.t2_bWD_leg2, pt.t2_bWD_leg1, pt.backApex1)"
            },
            {
                "type": "formula",
                "name": "bWD_extension_opposite_height",
                "formula": "Math.sin(pt.bWD_extension_angle) * dist(pt.backWaistCenter, pt.t2_bWD_leg1)"
            },
            {
                "type": "object",
                "name": "bWD_leg1",
                "object": "onLineAtLength(pt.t2_bWD_leg1, pt.backApex1, -pt.bWD_extension_opposite_height)"
            },
            {
                "type": "object",
                "name": "bWD_leg2",
                "object": "onLineAtLength(pt.t2_bWD_leg2, pt.backApex1, -pt.bWD_extension_opposite_height)"
            },
            {
                "type": "object",
                "name": "backWaistSide",
                "object": "intersectCircles(pt.backHighbustSide, 1*meas.side_waist_length, pt.bWD_leg2, 0.5*meas.back_waist_width - dist(pt.backWaistCenter, pt.bWD_leg1), 'y1 > y2')"
            },
            {
                "type": "object",
                "name": "bWD_point",
                "object": "copy(pt.backApex1)"
            },
            {
                "type": "object",
                "name": "p3",
                "object": "intersectLines(pt.backWaistCenter, pt.t1_bST, pt.backApex1, pt.bWD_leg1)"
            },
            {
                "type": "object",
                "name": "p4",
                "object": "onLineAtLength(pt.backApex1, pt.bWD_leg2, dist(pt.backApex1, pt.p3))"
            },
            {
                "type": "formula",
                "name": "t2_bST",
                "formula": "right(pt.t1_bST, dist(pt.p3, pt.p4))"
            },
            {
                "type": "object",
                "name": "bSD_mid",
                "object": "onLineAtLength(pt.backNeckSide, pt.t2_bST, dist(pt.backNeckSide, pt.t2_bST)/6)"
            },
            {
                "type": "object",
                "name": "backApex2",
                "object": "polar(pt.bSD_mid, 0.5*dist(pt.backNeckSide, pt.backAcrossCenter), angleBetween(pt.backNeckSide, pt.bSD_mid) + radians(90))"
            },
            {
                "type": "object",
                "name": "t1_bSD_leg2",
                "object": "onLineAtLength(pt.bSD_mid, pt.backNeckSide, dist(pt.p3, pt.p4)/2)"
            },
            {
                "type": "object",
                "name": "t1_bSD_leg1",
                "object": "onLineAtLength(pt.bSD_mid, pt.t2_bST, dist(pt.p3, pt.p4)/2)"
            },
            {
                "type": "object",
                "name": "bSD_point",
                "object": "polar(pt.backApex2, dist(pt.backApex2, pt.bSD_mid)/5, angleBetween(pt.backApex2, pt.bSD_mid))"
            },
            {
                "type": "formula",
                "name": "bSD_extension_theta",
                "formula": "radians(90) - angleBetween3pts(pt.t1_bSD_leg1, pt.t1_bSD_leg2, pt.backApex2)"
            },
            {
                "type": "formula",
                "name": "bSD_extension_opposite",
                "formula": "Math.sin(pt.bSD_extension_theta) * dist(pt.backNeckSide, pt.t1_bSD_leg2)"
            },
            {
                "type": "object",
                "name": "bSD_leg2",
                "object": "onLineAtLength(pt.t1_bSD_leg2, pt.backApex2, -pt.bSD_extension_opposite)"
            },
            {
                "type": "object",
                "name": "bSD_leg1",
                "object": "onLineAtLength(pt.t1_bSD_leg1, pt.backApex2, -pt.bSD_extension_opposite)"
            },
            {
                "type": "object",
                "name": "backShoulderTip",
                "object": "intersectLineCircle(pt.t1_bSD_leg1, pt.t2_bST, pt.bSD_leg1, dist(pt.t1_bSD_leg1, pt.t2_bST), 'x1 > x2')"
            },
            {
                "type": "point_xy",
                "name": "backArmscye1",
                "x": "pt.backShoulderTip.x - 0.6*Math.abs(pt.backShoulderTip.x - pt.backAcrossSide.x)",
                "y": "pt.backAcrossSide.y"
            },
            {
                "type": "point_xy",
                "name": "backArmscye2",
                "x": "pt.backAcrossSide.x + 0.5*Math.abs(pt.backAcrossSide.x - pt.backHighbustSide.x)",
                "y": "pt.backHighbustSide.y"
            },
            {
                "type": "point_xy",
                "name": "back_pattern_waist",
                "x": "pt.backWaistCenter.x + dist(pt.backWaistCenter, pt.bWD_leg1) + dist(pt.bWD_leg2, pt.backWaistSide)",
                "y": "pt.backWaistCenter.y + 2*pt.IN"
            },
            {
                "type": "point_xy",
                "name": "back_measured_waist",
                "x": "pt.backWaistCenter.x + 0.5*meas.back_waist_width",
                "y": "pt.backWaistCenter.y + 3*pt.IN"
            },
            {
                "type": "formula",
                "name": "bNC_in",
                "formula": "right(pt.backNeckCenter, 0.6*dist(pt.backNeckSide, pt.backNeckCenter))"
            },
            {
                "type": "object",
                "name": "bNS_out",
                "object": "polar(pt.backNeckSide, 0.15*dist(pt.backNeckSide, pt.backNeckCenter), angleBetween(pt.backNeckSide, pt.bNC_in))"
            },
            {
                "type": "object",
                "name": "bHBS_out",
                "object": "polar(pt.backHighbustSide, dist(pt.backHighbustSide, pt.backArmscye1)/2, angleBetween(pt.backHighbustSide, pt.backWaistSide) + radians(90))"
            },
            {
                "type": "object",
                "name": "bAS1_in",
                "object": "polar(pt.backArmscye1, dist(pt.backHighbustSide, pt.backArmscye1)/3, angleBetween(pt.backShoulderTip, pt.backArmscye2))"
            },
            {
                "type": "object",
                "name": "bAS1_out",
                "object": "polar(pt.backArmscye1, dist(pt.backArmscye1, pt.backShoulderTip)/3, angleBetween(pt.bAS1_in, pt.backArmscye1))"
            },
            {
                "type": "object",
                "name": "bST_in",
                "object": "polar(pt.backShoulderTip, dist(pt.backArmscye1, pt.backShoulderTip)/6, angleBetween(pt.backNeckSide, pt.backShoulderTip) + radians(90))"
            }
        ]
    }
}
