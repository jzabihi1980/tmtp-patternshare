        "a1": { "x": "pt.frontNeckCenter.x + pt.front_bust_ease", "y": "pt.frontNeckCenter.y + 0.1*meas.front_waist_length"},
        "a2": "right(pt.frontWaistCenter, pt.front_bust_ease)",
        "a3": "polar(pt.t1_fBD_leg2, pt.front_bust_ease, pt.frontSideAngle + radians(90))",
        "a4": "polar(pt.frontWaistSide, pt.front_waist_ease, pt.frontSideAngle + radians(90))",
        "a5": "onLineAtLength(pt.a4, pt.a3, 0.9*meas.side_waist_length)",
        "a6": "copy(pt.frontArmscye1)",
        "a7": "onLineAtLength(pt.frontWaistCenter, pt.frontShoulderTip, 1.02*meas.front_shoulder_balance)",
        "a8": "onLineAtLength(pt.frontWaistCenter, pt.frontNeckSide, 1.02*meas.front_neck_balance)",
        "a9": "onLineAtLength(pt.a8, pt.a7, 0.1*meas.shoulder_length)",
        "a1_in": "left(pt.a1, 0.8*Math.abs(pt.a1.x - pt.a9.x))",
        "a5_out": "polar(pt.a5, dist(pt.a5, pt.a6)/3, pt.frontSideAngle - radians(90))",
        "a6_in": "polar(pt.a6, dist(pt.a5, pt.a6)/2, angleBetween(pt.a7, pt.frontArmscye2))",
        "a6_out": "polar(pt.a6, dist(pt.a6, pt.a7)/3, angleBetween(pt.a6_in, pt.a6))",
        "a7_in": "polar(pt.a7, dist(pt.a6, pt.a7)/6, angleBetween(pt.a7, pt.a8) + radians(90))",
        "a9_out": "polar(pt.a9, 0.8*Math.abs(pt.a9.y - pt.a1_in.y), angleBetween(pt.a9, pt.a1_in))",

        "b1": { "x": "pt.backNeckCenter.x - pt.back_bust_ease", "y": "pt.backNeckCenter.y + 0.025*meas.back_waist_length"},
        "b2": "left(pt.backWaistCenter, pt.back_bust_ease)",
        "b3": "polar(pt.backHighbustSide, pt.back_bust_ease, pt.backSideAngle - radians(90))",
        "b4": "polar(pt.backWaistSide, pt.back_waist_ease, pt.backSideAngle - radians(90))",
        "b5": "onLineAtLength(pt.b4, pt.b3, 0.9*meas.side_waist_length)",
        "b6": "copy(pt.backArmscye1)",
        "b7": "onLineAtLength(pt.backWaistCenter, pt.backShoulderTip, 1.02*meas.back_shoulder_balance)",
        "b8": "onLineAtLength(pt.backWaistCenter, pt.backNeckSide, 1.02*meas.back_neck_balance)",
        "b9": "onLineAtLength(pt.b8, pt.b7, 0.1*meas.shoulder_length)",
        "b1_in": "right(pt.b1, 0.5*Math.abs(pt.b1.x - pt.b9.x))",
        "b5_out": "polar(pt.b5, dist(pt.b5, pt.b6)/3, pt.backSideAngle + radians(90))",
        "b6_in": "polar(pt.b6, dist(pt.b5, pt.b6)/2, angleBetween(pt.b7, pt.backArmscye2))",
        "b6_out": "polar(pt.b6, dist(pt.b6, pt.b7)/3, angleBetween(pt.b6_in, pt.b6))",
        "b7_in": "polar(pt.b7, dist(pt.b6, pt.b7)/6, angleBetween(pt.b7, pt.b8) - radians(90))",
        "b9_out": "polar(pt.b9, 0.8*Math.abs(pt.b9.y - pt.b1_in.y), angleBetween(pt.b7, pt.b8) - radians(90))",

        "c1": "right(pt.a1, Math.abs(pt.b5.y - pt.b7.y) + 1*pt.patternOffset + 2*pt.seamAllowance)",
        "c2": "down(pt.c1, Math.abs(pt.b5.x - pt.b7.x))",
        "c3": "down(pt.c1, 1*meas.overarm_length - 1*meas.underarm_length)",
        "c4": "left(pt.c2, Math.abs(pt.b5.y - pt.b7.y))",
        "c6": "right(pt.c2, Math.abs(pt.a5.y - pt.a7.y))",
        "c5": {"x": "pt.c1.x - Math.abs(pt.b6.y - pt.b7.y)", "y": "pt.c1.y + Math.abs(pt.b6.x - pt.b7.x)"},
        "c7": {"x": "pt.c1.x + Math.abs(pt.a6.y - pt.a7.y)", "y": "pt.c1.y + Math.abs(pt.a6.x - pt.a7.x)"},
        "c8": {"x":"pt.c1.x", "y":"pt.c1.y - Math.abs(pt.c1.y - Math.min(pt.c5.y, pt.c7.y))"},
        "c9": "up(pt.c8, 0.33*dist(pt.c1, pt.c2))",
        "c10": "down(pt.c3, 0.2*dist(pt.c1, pt.c2) + 0.1*meas.side_waist_length)",
        "c11": "down(pt.c4, 0.1*meas.side_waist_length)",
        "c12": "down(pt.c6, 0.1*meas.side_waist_length)",
        "c4_out": "right(pt.c4, dist(pt.c4, pt.c5)/3)",
        "c6_in": "left(pt.c6, dist(pt.c6, pt.c7)/3)",
        "c9_in": "left(pt.c9, dist(pt.c5, pt.c9)/3)",
        "c9_out": "right(pt.c9, dist(pt.c9, pt.c7)/3)",
        "c5_in": "polar(pt.c5, dist(pt.c4, pt.c5)/3, angleBetween(pt.c9_in, pt.c4_out))",
        "c5_out": "polar(pt.c5, dist(pt.c5, pt.c9)/3, angleBetween(pt.c4_out, pt.c9_in))",
        "c7_in": "polar(pt.c7, dist(pt.c9, pt.c7)/3, angleBetween(pt.c6_in, pt.c9_out))",
        "c7_out": "polar(pt.c7, dist(pt.c7, pt.c6)/3, angleBetween(pt.c9_out, pt.c6_in))",
        "c12_out": "left(pt.c12, dist(pt.c12, pt.c10)/3)",
        "c10_in": "right(pt.c10, dist(pt.c12, pt.c10)/3)",
        "c10_out": "left(pt.c10, dist(pt.c10, pt.c11)/3)",
        "c11_in": "right(pt.c11, dist(pt.c10, pt.c11)/3)",


        "front_pattern_waist": {"x": "pt.a2.x - (dist(pt.a2, pt.fWD_leg1) + dist(pt.fWD_leg2, pt.a4))", "y": "pt.a2.y + 2*pt.IN"},
        "front_measured_waist": {"x": "pt.a2.x - 0.5*meas.front_waist_width", "y": "pt.a2.y + 3*pt.IN"},

        "back_pattern_waist": {"x": "pt.b2.x + dist(pt.b2, pt.bWD_leg1) + dist(pt.bWD_leg2, pt.b4)", "y": "pt.b2.y + 2*pt.IN"},
        "back_measured_waist": {"x": "pt.b2.x + 0.5*meas.back_waist_width", "y": "pt.b2.y + 3*pt.IN"},

        "front_armscye_curve_length": "curveLength(pt.a5, pt.a5_out, pt.a6_in, pt.a6) + curveLength(pt.a6, pt.a6_out, pt.a7_in, pt.a7)",
        "back_armscye_curve_length": "curveLength(pt.b5, pt.b5_out, pt.b6_in, pt.b6) + curveLength(pt.b6, pt.b6_out, pt.b7_in, pt.b7)",
        "front_sleeve_curve_length": "curveLength(pt.c9, pt.c9_out, pt.c7_in, pt.c7) + curveLength(pt.c7, pt.c7_out, pt.c6_in, pt.c6)",
        "back_sleeve_curve_length": "curveLength(pt.c4, pt.c4_out, pt.c5_in, pt.c5) + curveLength(pt.c5, pt.c5_out, pt.c9_in, pt.c9)",

        "front_armscye_curve1": {"x": "pt.c6.x - curveLength(pt.a5, pt.a5_out, pt.a6_in, pt.a6)", "y": "pt.c10.y + 2*pt.IN"},
        "front_sleeve_curve1": {"x": "pt.c6.x - curveLength(pt.c7, pt.c7_out, pt.c6_in, pt.c6)", "y": "pt.c10.y + 2.5*pt.IN"},
        "front_armscye_curve": {"x": "pt.c6.x - pt.front_armscye_curve_length", "y": "pt.c10.y + 2*pt.IN"},
        "front_sleeve_curve": {"x": "pt.c6.x - pt.front_sleeve_curve_length", "y": "pt.c10.y + 2.5*pt.IN"},
        "back_armscye_curve": {"x": "pt.c6.x - pt.back_armscye_curve_length", "y": "pt.c10.y + 3*pt.IN"},
        "back_sleeve_curve": {"x": "pt.c6.x - pt.back_sleeve_curve_length", "y": "pt.c10.y + 3.5*pt.IN"},

        "d1": "down(pt.a2, 2*pt.seamAllowance + 1*pt.patternOffset)",
        "d2": "down(pt.d1, 1*meas.waist_to_floor - 1*meas.knee_to_floor)",
        "d3": "left(pt.d2, 1.2*meas.front_hip_width)",
        "d4": "up(pt.d3, dist(pt.d1, pt.d2))",

        "e1": "right(pt.d1, 2*pt.seamAllowance + 1*pt.patternOffset)",
        "e2": "down(pt.e1, 1*meas.waist_to_floor - 1*meas.knee_to_floor)",
        "e3": "right(pt.e2, 1.2*meas.front_hip_width)",
        "e4": "up(pt.e3, dist(pt.e1, pt.e2))",

        "f1": "down(pt.e3, 2*pt.seamAllowance + 1*pt.patternOffset)",
        "f2": "down(pt.f1, 0.75*meas.side_waist_length)",
        "f3": "left(pt.f2, 2*dist(pt.d1, pt.d4))",
        "f4": "up(pt.f3, dist(pt.f1, pt.f2))",

        "g1": {"x": "pt.a1.x + 2*pt.seamAllowance + 1*pt.patternOffset + 1*meas.front_highbust_height", "y": "pt.a1.y - 2*pt.seamAllowance - 1*pt.patternOffset"},
        "g2": "down(pt.g1, 0.4*meas.front_highbust_height)",
        "g4": "polar(pt.g1, dist(pt.a1, pt.a9), angleBetween(pt.a1, pt.a9))",
        "g3": "polar(pt.g4, dist(pt.g1, pt.g2), angleBetween(pt.a9, pt.a7))",
        "g1_in": "left(pt.g1, dist(pt.a1, pt.a1_in))",
        "g2_out": "left(pt.g2, 0.8*Math.abs(pt.g2.x - pt.g3.x))",
        "g3_in": "polar(pt.g3, 0.8*Math.abs(pt.g2.y - pt.g3.y), angleBetween(pt.a9, pt.a9_out) )",
        "g4_out": "polar(pt.g4, dist(pt.a9, pt.a9_out), angleBetween(pt.a9, pt.a9_out))",

        "h1": {"x": "pt.b7.x + 2*pt.seamAllowance + 1*pt.patternOffset", "y": "pt.b7.y"},
        "h2": "down(pt.h1, 0.4*meas.front_highbust_height)",
        "h4": "polar(pt.h1, dist(pt.b1, pt.b9), angleBetween(pt.b1, pt.b9))",
        "h3": "polar(pt.h4, dist(pt.h1, pt.h2), angleBetween(pt.b9, pt.b7))",
        "h1_in": "right(pt.h1, dist(pt.b1, pt.b1_in))",
        "h2_out": "right(pt.h2, 0.5*Math.abs(pt.h2.x - pt.h3.x))",
        "h3_in": "polar(pt.h3, 0.8*Math.abs(pt.h2.y - pt.h3.y), angleBetween(pt.b9, pt.b9_out) )",
        "h4_out": "polar(pt.h4, dist(pt.b9, pt.b9_out), angleBetween(pt.b9, pt.b9_out))"
    }

