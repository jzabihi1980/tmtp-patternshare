    "a14" : "left(pt.a13, 1*meas.across_chest/2.0)",
    "a15" : { "x" : "pt.a8.x", "y" : "pt.a2.y" },

    "b1": { "x" : "pt.a1.x + 3*pt.seamAllowance + 0.5*pt.IN", "y" : "pt.a1.y" },
    "b2": "down(pt.b1, 1*meas.back_shoulder_height)",
    "b3": "right(pt.b2, 1*meas.back_bust_width/2 + 0.5*pt.IN)",
    "b4": "right(pt.b1,1*meas.back_shoulderwidth_balance + 0.25*pt.IN)",
    "b5": "up(pt.b2, 1*meas.back_waist_length)",
    "b6": { "x" : "pt.b2.x + 1*meas.back_waist_width/4 + 0.5*pt.IN", "y" : "pt.b2.y + Math.floor(Math.max((1*meas.bust - 24*pt.IN), 0)/2)/16" },
    "b7": "right(pt.b6, 0.75*pt.IN)",
    "b8": "midPoint(pt.b6, pt.b7)",
    "b9": "up(pt.b8, 1*meas.side_waist_length - 0.5*pt.IN)",
    "b10" : "onCircleAtX(pt.b2, 1*meas.back_shoulderheight_balance + 0.25*pt.IN, pt.b4.x, 'y1 < y2')",
