    "CM" : "convertUnit(1, 'cm', window.measurementData.clientdata.measurement_units)",
    "IN" : "convertUnit(1, 'in', window.measurementData.clientdata.measurement_units)",
    "seamAllowance" : "(5/8)*pt.IN",
    "a1": { "x": "pt.seamAllowance + 1*meas.front_bust_width/2", "y": "1*pt.IN" },
    "a2": "down(pt.a1, 1*meas.front_shoulder_height)",
    "a3": "left(pt.a2, 1*meas.front_bust_width/2.0 + 1*pt.IN) ",
    "a4": "left(pt.a1, 1*meas.front_shoulderwidth_balance)",
    "a5": "up(pt.a2, 1*meas.front_waist_length)",
    "a6" : "onCircleAtX(pt.a2, 1*meas.front_shoulderheight_balance + 0.25*pt.IN, pt.a4.x, 'y1 < y2')",
    "a7": "onCircleAtY(pt.a6, 1*meas.shoulder_length, pt.a4.y, 'x1 > x2')",
