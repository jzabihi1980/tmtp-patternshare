  "points": {
    "CM" : "convertUnit(1, 'cm', window.measurementData.clientdata.units)",
    "a1": { "x": "0", "y": "0" },
    "a2": "down(pt.a1, 1*meas.back_waist_length)",
    "a3": "down(pt.a2, 1*meas.back_hip_height)",
    "a4": "down(pt.a1, 1*meas.back_underarm_height)",
    "a5": "onCircleAtX(pt.a2, 1*meas.back_neck_balance, pt.a1.x + 0.5*meas.neck_width, 'y1 < y2')",
    "temp_a6": "intersectCircles(pt.a1, 1*meas.back_shoulder_balance, pt.a5, 1*meas.shoulder_length, 'y1 > y2')",
    "a6" : "right(pt.temp_a6, 0.75*pt.CM)",
    "a7": "down(pt.a1, 0.5*dist(pt.a1, pt.a4))",
    "a8": "right(pt.a7, 0.25*meas.back_upperchest_width + 0.5*pt.a6.x + 1*pt.CM)",
    "a9": { "x" : "pt.a8.x", "y" : "pt.a4.y" },
    "a10": "right(pt.a4, 0.5*meas.back_bust_width + 2.5*pt.CM)",
    "a11": { "x": "pt.a10.x", "y": "pt.a2.y" },
    "a12": { "x": "pt.a10.x", "y": "pt.a3.y" },
    "b1": { "x" : "2*pt.a10.x + 6*pt.CM", "y" : "pt.a2.y - 1*meas.front_waist_length" },
    "b2": { "x" : "pt.b1.x", "y" : "pt.a2.y" },
    "b3": { "x" : "pt.b1.x", "y" : "pt.a3.y" },
    "b4": { "x" : "pt.b1.x", "y" : "pt.a4.y" },
    "b5": "onCircleAtX(pt.b2, 1*meas.front_neck_balance, pt.b1.x - 0.5*meas.neck_width,'y1 < y2')",
    "temp_b6" : "intersectCircles(pt.b1, 1*meas.front_shoulder_balance, pt.b5, 1*meas.shoulder_length, 'y1 > y2')",
    "b6": "left(pt.temp_b6, 0.75*pt.CM)",
    "b7": "down(pt.b1, 0.5*dist(pt.b1, pt.b4))",
    "temp_b8_a" : "left(pt.b7, 0.5*meas.front_upperchest_width)",
    "temp_b8_b" : { "x" : "pt.b6.x", "y" : "pt.b7.y" },
    "b8": { "x" : "0.5*(pt.temp_b8_a.x + pt.temp_b8_b.x) - 1*pt.CM", "y" : "pt.b7.y" },
    "b9": { "x": "pt.b8.x", "y": "pt.b4.y" },
    "b10" : "left(pt.b4, 0.5*meas.front_bust_width + 2.5*pt.CM)",
    "b11" : { "x" : "pt.b10.x", "y" : "pt.b2.y" },
    "b12" : { "x" : "pt.b10.x", "y" : "pt.b3.y" },
    "a1in" : "right(pt.a1, dist(pt.a1, pt.a5)/2)",
    "a5out" : "polar(pt.a5, dist(pt.a1, pt.a5)/6, angleBetween(pt.a5, pt.a6) + radians(90))",
    "a10out" : "left(pt.a10, dist(pt.a10, pt.a8)/3)",
    "a8in" : "polar(pt.a8, dist(pt.a10, pt.a8)/3, 0.5*(angleBetween(pt.a6, pt.a10) + angleBetween(pt.a6, pt.a8)))",
    "a8out" : "polar(pt.a8, dist(pt.a8, pt.a6)/3, angleBetween(pt.a8in, pt.a8))",
    "a6in" : "polar(pt.a6, dist(pt.a8, pt.a6)/6, angleBetween(pt.a5, pt.a6) + radians(90))",
    "b1in" : "left(pt.b1, dist(pt.b1, pt.b5)/2)",
    "b5out" : "polar(pt.b5, dist(pt.b1, pt.b5)/6, angleBetween(pt.b5, pt.b6) - radians(90))",
    "b10out" : "right(pt.b10, dist(pt.b10, pt.b8)/3)",
    "b8in" : "polar(pt.b8, dist(pt.b10, pt.b8)/3, 0.5*(angleBetween(pt.b6, pt.b10) + angleBetween(pt.b6, pt.b8)))",
    "b8out" : "polar(pt.b8, dist(pt.b8, pt.b6)/3, angleBetween(pt.b8in, pt.b8))",
    "b6in" : "polar(pt.b6, dist(pt.b8, pt.b6)/6, angleBetween(pt.b5, pt.b6) - radians(90))"
