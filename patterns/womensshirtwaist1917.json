{
  "pattern": {
    "title": "Women's Shirt Waist Block - circa 1917",
    "defaults": [ "42", "37", "100", "70", "12", "41.3", "34", "21.6", "25.7", "87", "34.9", "25.4", "20" ],
    "measurements": [ "waist_length", "neck", "chest", "waist", "shoulder", "armscye", "back_width", "side", "upper_front_height", "sleeve_length","elbow_height", "elbow_girth", "hand_girth"],
    "main": [
      {
        "id": "Back",
        "type": "path",
        "d": [
          ["M", ["pt.B.x", "pt.B.y"] ],
          ["L", ["pt.A.x","pt.A.y"] ],
          ["L", ["pt.V.x","pt.V.y"] ],
          ["L", ["pt.T.x","pt.T.y"] ],
          ["L", ["pt.I.x","pt.I.y"] ],
          ["L", ["pt.H.x","pt.H.y"] ],
          ["z"]
        ],
        "drawattr": {},
        "appearanceattr": { "stroke-width":"3", "stroke":"#000000", "fill":"none" }
      },
      {
        "id": "Front",
        "type": "path",
        "d": [
          ["M", ["pt.S.x", "pt.S.y"] ],
          ["L", ["pt.D.x","pt.D.y"] ],
          ["L", ["pt.X.x","pt.X.y"] ],
          ["L", ["pt.T.x","pt.T.y"] ],
          ["L", ["pt.K.x","pt.K.y"] ],
          ["L", ["pt.L.x","pt.L.y"] ],
          ["L", ["pt.N.x","pt.N.y"] ],
          ["L", ["pt.P.x","pt.P.y"] ],
          ["L", ["pt.O.x","pt.O.y"] ],
          ["L", ["pt.S.x","pt.S.y"] ],
          ["z"]
        ],
        "drawattr": {},
        "appearanceattr": { "stroke-width":"3", "stroke":"#000000", "fill":"none" }
      },
      {
        "id": "Sleeve",
          "type": "path",
          "d": [
            ["M", ["pt.SF.x", "pt.SF.y"] ],
            ["L", ["pt.SR.x","pt.SR.y"] ],
            ["L", ["pt.SU.x","pt.SU.y"] ],
            ["L", ["pt.SS.x","pt.SS.y"] ],
            ["L", ["pt.SM.x","pt.SM.y"] ],
            ["L", ["pt.SN.x","pt.SN.y"] ],
            ["L", ["pt.SL.x","pt.SL.y"] ],
            ["L", ["pt.SI.x","pt.SI.y"] ],
            ["L", ["pt.SY.x","pt.SY.y"] ],
            ["L", ["pt.SV.x","pt.SV.y"] ],
            ["L", ["pt.SD.x","pt.SD.y"] ],
            ["L", ["pt.SE.x","pt.SE.y"] ],
            ["z"]
          ],
          "drawattr": {},
          "appearanceattr": { "stroke-width":"3", "stroke":"#000000", "fill":"none" }
      }
    ],
    "construction": [
      {
        "id": "armscye line",
        "type": "path",
        "d": [
              ["m", ["pt.B.x","pt.B.y"] ],
              ["L", ["pt.C.x","pt.C.y"] ]
        ],
        "drawattr": {},
        "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
      },
      {
        "id": "waist line",
        "type": "path",
        "d": [
          ["m", ["pt.E.x","pt.E.y"] ],
          ["L", ["pt.F.x","pt.F.y"] ]
        ],
        "drawattr": {},
        "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
      },
      {
        "id": "mid line",
        "type": "path",
        "d": [
          ["m", ["pt.M.x","pt.M.y"] ],
          ["L", ["pt.O.x","pt.O.y"] ]
        ],
        "drawattr": {},
        "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
      },
      {
        "id": "elbow line",
        "type": "path",
        "d": [
          ["M", ["pt.k.x","pt.z.y"] ],
          ["L", ["pt.l.x","pt.z.y"] ]
        ],
        "drawattr": {},
        "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
      },
      {
        "id": "arm line",
        "type": "path",
        "d": [
          ["m", ["pt.i.x","pt.i.y"] ],
          ["L", ["pt.m.x","pt.m.y"] ]
        ],
        "drawattr": {},
        "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
      },
      {
        "id": "arm hole for sleeve",
        "type": "path",
        "d": [
          ["M", ["pt.g.x","pt.g.y"] ],
          ["L", ["pt.e.x","pt.e.y"] ],
          ["L", ["pt.h.x","pt.h.y"] ],
          ["L", ["pt.b.x","pt.b.y"] ],
          ["L", ["pt.f.x","pt.f.y"] ]
        ],
        "drawattr": {},
        "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
      }
    ],
    "points": {
      "B": { "x" : "0", "y" : "0" },
      "A": { "x" : "pt.B.x", "y" : "pt.B.y + meas.waist_length" },
      "C": { "x" : "pt.A.x", "y" : "pt.A.y - meas.side" },
      "D": { "x" : "pt.C.x - (meas.chest / 2.0)", "y" : "pt.C.y" },
      "E": { "x" : "pt.C.x - (meas.back_width / 2.0)", "y" : "pt.C.y" },
      "F": { "x" : "pt.E.x", "y" : "pt.E.y - (meas.armscye / 3.0)" },
      "G": { "x" : "pt.B.x", "y" : "pt.B.y - (meas.waist_length / 33.07)" },
      "H": { "x" : "pt.G.x - (meas.neck / 6.0)", "y" : "pt.G.y" },
      "I": { "x" : "pointOnLineAtLength(pt.F, pt.H, (-meas.shoulder / 9.44), 'x')",
             "y" : "pointOnLineAtLength(pt.F, pt.H, (-meas.shoulder / 9.44), 'y')" },
      "J": { "x" : "pt.E.x - (meas.armscye / 4.0)", "y" : "pt.E.y" },
      "K": { "x" : "midPoint(pt.J, pt.E, 'x')", "y" : "midPoint(pt.J, pt.E, 'y')" },
      "L": { "x" : "pt.J.x", "y" : "pt.J.y - dist(pt.F, pt.E) / 2.0" },
      "M": { "x" : "pt.F.x", "y" : "pt.F.y - (2 / 3.0) * (pt.F.y - pt.B.y)" },
      "N": { "x" : "pt.K.x", "y" : "pt.M.y" },
      "O": { "x" : "pt.M.x - dist(pt.H, pt.I)", "y" : "pt.M.y" },
      "P": { "x" : "pt.O.x", "y" : "pt.C.y - meas.upper_front_height" },
      "Q": { "x" : "pt.P.x", "y" : "pt.P.y + meas.neck / 4.0" },
      "R": { "x" : "pt.Q.x", "y" : "pt.Q.y - meas.neck / 29.13" },
      "S": { "x" : "pt.R.x - (meas.neck / 5.0)", "y" : "pt.R.y" },
      "T": { "x" : "pt.E.x - (meas.back_width / 13.39)", "y" : "pt.C.y" },
      "U": { "x" : "pt.T.x", "y" : "pt.A.y" },
      "V": { "x" : "pt.E.x", "y" : "pt.A.y" },
      "W": { "x" : "pt.U.x - dist(pt.U, pt.V)", "y" : "pt.A.y" },
      "X": { "x" : "pointOnLineAtLength(pt.S, pt.D, meas.waist_length, 'x')", "y" : "pointOnLineAtLength(pt.S, pt.D, meas.waist_length, 'y')" },

      "SA": { "x": "0", "y": "0" },
      "SB": { "x": "pt.SA.x", "y": "pt.SA.y + meas.sleeve_length" },
      "SC": { "x": "pt.SA.x", "y": "pt.SA.y - meas.elbow_height" },
      "SD": { "x": "pt.SB.x + (meas.armscye / 16.26)", "y": "pt.SB.y" },
      "SE": { "x": "pt.SC.x + (meas.armscye / 32.52)", "y": "pt.SC.y" },
      "SF": { "x": "pt.SA.x - (meas.armscye / 16.26)", "y": "pt.SA.y" },
      "SG": { "x": "pt.SD.x + (meas.armscye / 16.26)", "y": "pt.SD.y" },
      "SH": { "x": "pt.SG.x + (meas.hand_girth * 1.00635)", "y": "pt.SG.y" },
      "SI": { "x": "pt.SH.x + (meas.armscye / 16.26)", "y": "pt.SH.y" },
      "SJ": { "x": "pt.SE.x + (meas.armscye / 16.26)", "y": "pt.SE.y" },
      "SK": { "x": "pt.SJ.x + (meas.elbow_girth * 1.0)", "y": "pt.SJ.y" },
      "SL": { "x": "pt.SK.x + (meas.armscye / 32.52)", "y": "pt.SK.y" },
      "SM": { "x": "pt.SA.x + meas.armscye", "y": "pt.SA.x" },
      "SN": { "x": "pt.SM.x + 2 * (meas.armscye / 16.26)", "y": "pt.SM.x" },
      "SO": { "x": "pt.SA.x", "y": "pt.SA.y - 2.5 * (meas.sleeve_length / 34.25)" },
      "SP": { "x": "pt.SA.x + 1.5 * (meas.armscye / 16.26)", "y": "pt.SA.y" },
      "SQ": { "x": "pt.SM.x - 3 * (meas.armscye / 16.26)", "y": "pt.SM.y" },
      "SR": { "x": "pt.SP.x", "y": "pt.SO.y" },
      "SS": { "x": "pt.SQ.x", "y": "pt.SO.y" },
      "ST": { "x": "midPoint(pt.SP, pt.SQ, 'x')", "y": "midPoint(pt.SP, pt.SQ, 'y')" },
      "SU": { "x": "pt.ST.x", "y": "pt.ST.y - dist(pt.T, pt.R)" },
      "SV": { "x": "pt.SG.x", "y": "pt.SG.y + dist(pt.SG, pt.SH)/2.0 + .75 * (meas.sleeve_length / 34.25)" },
      "SW": { "x": "pt.SD.x + dist(pt.D, pt.H) * (3/5.0)", "y": "pt.SD.y" },
      "SX": { "x": "pt.SW.x", "y": "pt.SW.y - dist(pt.D, pt.C)/3.0" },
      "SY": { "x": "pt.SW.x", "y": "pt.SW.y + 0.75 * (meas.sleeve_length / 34.25)" }
    }
  }
}

