        {
          "id" : "Coat Pocket Flap H",
          "type" : "path",
          "d" : [
            ["M", "pt.h1" ],
            ["L", "pt.h2" ],
            ["L", "pt.h3" ],
            ["L", "pt.h4" ],
            ["L", "pt.h1" ],
            ["z"]
          ],
          "drawattr": {},
          "appearanceattr": { "stroke-width":"3", "stroke":"#000000", "fill":"none" }
        },
        {
          "id" : "Coat Hood Side D",
          "type" : "path",
          "d" : [
            ["M", "pt.d1" ],
            ["C", "pt.d1out", "pt.d6in", "pt.d6" ],
            ["C", "pt.d6out", "pt.d7in", "pt.d7" ],
            ["L", "pt.d4" ],
            ["L", "pt.d8" ],
            ["C", "pt.d8out", "pt.d9in", "pt.d9" ],
            ["L", "pt.d10" ],
