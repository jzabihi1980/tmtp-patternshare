    "points": {
        "CM": "convertUnit(1, 'cm', window.measurementData.clientdata.units)",
        "IN": "convertUnit(1, 'in', window.measurementData.clientdata.units)",
        "seamAllowance": "(5/8)*pt.IN",
        "patternOffset": "1*pt.IN",

        "backNeckCenter": { "x": "2*pt.patternOffset", "y": "2*pt.patternOffset" },
        "backWaistCenter": { "x": "0", "y": "1*meas.back_waist_length" },
        "t1_backHemCenter" : "down(pt.backNeckCenter, 1*meas.height - 1*meas.head_and_neck_length)",
        "backHemCenter" : "polar(pt.backWaistCenter, dist(pt.backWaistCenter, pt.t1_backHemCenter), radians(105))",
        "backHipCenter" : "onLineAtLength(pt.backWaistCenter, pt.backHemCenter, 1*meas.side_hip_height)" ,
        "backNeckSide" : "onCircleAtX(pt.backWaistCenter, 1*meas.back_neck_balance, 0.5*meas.neck_diameter, 'y1 < y2')" ,
        "t1_bST": "intersectCircles(pt.backNeckSide, 1*meas.shoulder_length, pt.backWaistCenter, 1*meas.back_shoulder_balance, 'y1 > y2')",
        "backUnderarmSide": { "x": "0.55*meas.across_back", "y": "(pt.t1_bST.y + 1*meas.back_bust_height)/2" },
        "backHighbustSide" : { "x": "0.55*meas.back_bust_width", "y": "1.0*meas.back_highbust_height" },
        "t1_bWS" : { "x": "pt.backHighbustSide.x", "y" : "pt.backWaistCenter.y" },
        "backBustSide": { "x": "0.55*meas.back_bust_width", "y": "1.0*meas.back_bust_height" },
        "t2_bWS" : "intersectCircles(pt.backHighbustSide, 1.0*meas.side_waist_length, pt.backWaistCenter, 0.55*meas.back_waist_width, 'y1 > y2')",
        "backHemSide" : "polar(pt.t1_bWS, dist(pt.backWaistCenter, pt.t1_backHemCenter), radians(70))",
        "backHipSide" : "onLineAtLength(pt.t2_bWS, pt.backHemSide, 1*meas.side_hip_height)",
        "backWaistSide" : "right(pt.t2_bWS, 0.05*dist(pt.backWaistCenter, pt.t2_bWS))",
        "t2_bST" : "polar(pt.t1_bST, 0.2*meas.shoulder_length, radians(315))",
        "backShoulderTip" : "up(pt.t2_bST, 0.2*meas.shoulder_length)",
        "bNCin" : "right(pt.backNeckCenter, dist(pt.backNeckCenter, pt.backNeckSide)/2)",
        "bWCout" : "down(pt.backWaistCenter, dist(pt.backWaistCenter, pt.backHipCenter)/3)",
        "backHemCenterout" : "polar(pt.backHemCenter, dist(pt.backHemCenter, pt.backHemSide)/3, angleBetween(pt.backHipCenter, pt.backHemCenter) - radians(90) )",
        "bHSin" : "onLineAtLength(pt.backWaistCenter, pt.backHipCenter, dist(pt.backWaistCenter, pt.backHipCenter)/3)",
        "bNSout" : "polar(pt.backNeckSide, dist(pt.backNeckCenter,pt.backNeckSide)/6, angleBetween(pt.backNeckSide, pt.backShoulderTip) + radians(90) )",
        "bSTin" : "polar(pt.backShoulderTip, dist(pt.backShoulderTip, pt.backUnderarmSide)/3, angleBetween(pt.backNeckSide, pt.backShoulderTip) + radians(90) )",
        "bBSin" : "polar(pt.backBustSide, dist(pt.backBustSide, pt.backWaistSide)/3, angleBetween(pt.backBustSide, pt.backWaistSide))",
        "bBSout" : "left(pt.backBustSide, dist(pt.backBustSide, pt.backUnderarmSide)/3)",
        "bUSin"  : "polar(pt.backUnderarmSide, dist(pt.backBustSide,pt.backUnderarmSide)/3.0, 0.5*(angleBetween(pt.bSTin,pt.backUnderarmSide) + angleBetween(pt.backUnderarmSide,pt.bBSout)))",
        "bUSout" : "polar(pt.backUnderarmSide, dist(pt.backUnderarmSide,pt.backShoulderTip)/3.0, angleBetween(pt.bUSin, pt.backUnderarmSide))",
        "bHipSout" : "polar(pt.backHipSide, dist(pt.backHipSide, pt.backWaistSide)/3, angleBetween(pt.backHipSide, pt.backWaistSide) )",
        "bHemSin" : "polar(pt.backHemSide, dist(pt.backHemSide, pt.backHemCenter)/3, angleBetween(pt.backHipSide, pt.backHemSide) + radians(90))",
        "bWSin" : "down(pt.backWaistSide, dist(pt.backWaistSide, pt.backHipSide)/6)",
        "bWSout" : "up(pt.backWaistSide, dist(pt.backBustSide, pt.backWaistSide)/3)",


        "b2" : "right(pt.backWaistCenter, 2.10*pt.backHemSide.x)",
        "b1" : "up(pt.b2, 1*meas.front_waist_length)",
        "b_floor_length" : "down(pt.b1, 1*meas.height - 1*meas.head_and_neck_length)",
        "b3" : "polar(pt.b2, dist(pt.b2, pt.b_floor_length), radians(70))",
        "b4" : "polar(pt.b2, 1*meas.side_hip_height, angleBetween(pt.b2, pt.b3))",
        "b5" : "onCircleAtX(pt.b2, 1*meas.front_neck_balance, pt.b1.x - 0.5*meas.neck_diameter, 'y1 < y2')" ,
        "b6" : "intersectCircles(pt.b5, 1*meas.shoulder_length, pt.b2, 1*meas.front_shoulder_balance, 'y1 > y2')",
        "b7" : { "x": "pt.b1.x - (0.6*meas.across_chest)", "y": "(pt.b1.y + 0.75*meas.front_highbust_height)" },
        "temp_b8" : { "x": "pt.b1.x - 0.55*meas.front_bust_width", "y": "(pt.b1.y + 1.0*meas.front_highbust_height)" },
        "temp_b9" : { "x": "pt.temp_b8.x", "y" : "pt.b2.y" },
        "b8": { "x": "pt.temp_b8.x", "y": "(pt.b6.y + 1.0*meas.front_bust_height)" },
        "b9" : "intersectCircles(pt.temp_b8, 1.0*meas.side_waist_length, pt.b2, 0.5*meas.front_waist_width, 'y1 > y2')",
        "b11" : "polar(pt.temp_b9, dist(pt.b2, pt.b_floor_length), radians(110))",
        "b10" : "onLineAtLength(pt.b9, pt.b11, 1*meas.side_hip_height)",
        "b12" : "left(pt.b9, 0.02*dist(pt.b2, pt.b9))",
        "b13" : "down(pt.b1, 0.5*pt.b7.y)",
        "b14" : "left(pt.b2, 0.6*dist(pt.b1, pt.b6))",
        "b15" : "down(pt.b14, 0.25*dist(pt.b9, pt.b10))",
        "b17" : { "x": "pt.b15.x", "y": "pt.b15.y + 1.15*meas.hand_length" },
        "temp_b18" : "left(pt.b17, 0.5*dist(pt.b12, pt.b10))",
        "b18" : "intersectLines(pt.b17, pt.temp_b18, pt.b10, pt.b11)",
        "b19" : { "x": "pt.b1.x - 0.75*(pt.b1.x - pt.b7.x)", "y": "pt.b8.y" },
        "b20_temp" : "polar(pt.b6, 0.2*meas.shoulder_length, radians(225))",
        "b20" : "up(pt.b20_temp, 0.2*meas.shoulder_length)",
        "b1in" : "left(pt.b1, dist(pt.b1, pt.b5)/2)",
        "b2out" : "down(pt.b2, dist(pt.b2, pt.b4)/3)",
        "b3out" : "polar(pt.b3, dist(pt.b3, pt.b11)/3, angleBetween(pt.b4, pt.b3) + radians(90) )",
        "b4in" : "onLineAtLength(pt.b3, pt.b4, dist(pt.b2, pt.b4)/3)",
        "b5out" : "polar(pt.b5, dist(pt.b1,pt.b5)/3, angleBetween(pt.b5, pt.b20) - radians(90) )",
        "b20in" : "polar(pt.b20, dist(pt.b20, pt.b7)/3, angleBetween(pt.b5, pt.b20) - radians(90) )",
        "b8in" : "polar(pt.b8, dist(pt.b8, pt.b12)/3, angleBetween(pt.b8, pt.b12))",
        "b8out" : "right(pt.b8, dist(pt.b8, pt.b7)/2.0)",
        "b7in"  : "polar(pt.b7, dist(pt.b8,pt.b7)/3.0, 0.5*(angleBetween(pt.b20in,pt.b7) + angleBetween(pt.b7,pt.b8out)))",
        "b7out"  : "polar(pt.b7, dist(pt.b7,pt.b20)/3.0, angleBetween(pt.b7in, pt.b7))",
        "b10out" : "polar(pt.b10, dist(pt.b10, pt.b12)/3, angleBetween(pt.b11, pt.b10))",
        "b11in" : "polar(pt.b11, dist(pt.b11, pt.b3)/3, angleBetween(pt.b10, pt.b11) - radians(90))",
        "b12in" : "polar(pt.b12, dist(pt.b12, pt.b10)/6, angleBetween(pt.b8, pt.b10))",
        "b12out" : "polar(pt.b12, dist(pt.b8, pt.b12)/3, angleBetween(pt.b10, pt.b8))",
        "b13out" : "polar(pt.b13, 0.3*dist(pt.b1, pt.b8),1.1*angleBetween(pt.b13, pt.b19))",
        "b14in" : "up(pt.b14, 0.3*dist(pt.b14, pt.b7))",
        "b14out" : "down(pt.b14, 0.3*dist(pt.b14, pt.b15))",
        "b15in" : "polar(pt.b15, 0.3*dist(pt.b14, pt.b15), angleBetween(pt.b15, pt.b14out))",
        "b19in" : "up(pt.b19, 0.5*dist(pt.b19, pt.b7))",
        "b19out" : "down(pt.b19, 0.3*dist(pt.b19, pt.b14))",
        "c1" : "right(pt.b13, 2*dist(pt.b7, pt.b5))",
        "c2" : "right(pt.b2, 2*dist(pt.b7, pt.b5))",
        "c3" : "right(pt.b3, 2*dist(pt.b7, pt.b5))",
        "c3out" : "right(pt.b3out, 2*dist(pt.b7, pt.b5))",
        "c4in" : "right(pt.b11in, 2*dist(pt.b7, pt.b5))",
        "c4" : "right(pt.b11, 2*dist(pt.b7, pt.b5))",
        "c5" : "right(pt.b10, 2*dist(pt.b7, pt.b5))",
        "c6" : "right(pt.b15, 2*dist(pt.b7, pt.b5))",
        "c6out" : "right(pt.b15in, 2*dist(pt.b7, pt.b5))",
        "c7in" : "right(pt.b14out, 2*dist(pt.b7, pt.b5))",
        "c7" : "right(pt.b14, 2*dist(pt.b7, pt.b5))",
        "c7out" : "right(pt.b14in, 2*dist(pt.b7, pt.b5))",
        "c8in" : "right(pt.b19out, 2*dist(pt.b7, pt.b5))",
        "c8" : "right(pt.b19, 2*dist(pt.b7, pt.b5))",
        "c8out" : "right(pt.b19in, 2*dist(pt.b7, pt.b5))",
        "c1in" : "right(pt.b13out, 2*dist(pt.b7, pt.b5))",
        "d1" : { "x": "pt.b5.x", "y": "2*pt.b5.y" },
        "d2" : "right(pt.d1, 0.80*(curveLength(pt.backNeckSide,pt.bNSout,pt.bNCin,pt.backNeckCenter) + curveLength(pt.b5,pt.b5out,pt.b1in,pt.b1)))",
        "d3" : "up(pt.d1, 1.3*meas.head_and_neck_length)",
        "d4" : "up(pt.d2, 1.4*meas.head_and_neck_length)",
        "d5" : "midPoint(pt.d1, pt.d3)",
        "d6" : "left(pt.d5, 0.15*meas.head_and_neck_length)",
        "d7" : "midPoint(pt.d3, pt.d4)",
        "d8" : "down(pt.d4, 0.75*dist(pt.d1, pt.d3))",
        "d9" : {"x":"pt.d2.x + 0.25*dist(pt.d1, pt.d2)", "y": "pt.d2.y"},
        "d10" : "polar(pt.d1, dist(pt.d1, pt.d2), 0.5*angleBetween(pt.b5, pt.b1))",
        "d1out" : "up(pt.d1, dist(pt.d1, pt.d6)/6)",
        "d6in" : "down(pt.d6, dist(pt.d6, pt.d1)/3)",
        "d6out" : "up(pt.d6, dist(pt.d6, pt.d7)/3)",
        "d7in" : "left(pt.d7, dist(pt.d6, pt.d7)/3)",
        "d8out" : "down(pt.d8, dist(pt.d8, pt.d9)/3)",
        "d9in" : "polar(pt.d9, dist(pt.d8, pt.d9)/3, angleBetween(pt.d9, pt.d10) + radians(90))",
        "e1" : { "x" : "pt.b7.x", "y" : "pt.d1.y" },
        "e2" : "left(pt.e1, dist(pt.d1, pt.d2)/3)",
        "e3" : "up(pt.e2, curveLength(pt.d1, pt.d1out, pt.d6in, pt.d6) + curveLength(pt.d6, pt.d6out, pt.d7in, pt.d7) + dist(pt.d7, pt.d4))",
