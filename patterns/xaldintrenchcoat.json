{
  "pattern": {
    "title": "Xaldin Trench Coat",
    "defaults" : [
        8.0,
        17.375,
        19.75,
        6.625,
        9.125,
        0,
        8.125,
        7.25,
        38.0,
        4.0,
        0,
        23.0,
        0,
        10.0,
        17.0,
        19.625,
        52.0,
        26.0,
        26.0,
        48.5,
        26.5,
        22,
        49,
        24.5,
        24.5
    ],
    "measurements": [
        "neck_width",
        "center_front_waist_length",
        "center_back_waist_length",
        "shoulder_length",
        "side_waist_length",
        "front_upperbust_height",
        "Scye_depth",
        "waist_to_hip_height",
        "hip_height",
        "ankle_height",
        "center_front_waist_to_neck_point_balance",
        "center_back_waist_to_neck_point_balance",
        "front_neck_hollow_to_shoulder_point_balance",
        "cervicale_to_shoulder_point_balance",
        "upper_front_chest_width",
        "upper_back_width",
        "bust_girth",
        "front_bust_width",
        "back_bust_width",
        "waist_girth",
        "front_waist_width",
        "back_waist_width",
        "hip_girth",
        "front_hip_width",
        "back_hip_width"
    ],

    "main" : [
        {
          "id" : "Coat Back",
          "type" : "path",
          "d" : [
            ["M", ["pt.a1.x","pt.a1.y"] ],
            ["L", ["pt.a2.x","pt.a2.y"] ],
            ["C", ["pt.a2out.x","pt.a2out.y"], ["pt.a4in.x","pt.a4in.y"], ["pt.a4.x","pt.a4.y"] ],
            ["L", ["pt.a3.x","pt.a3.y"] ],
            ["C", ["pt.a3out.x","pt.a3out.y"], ["pt.a11in.x","pt.a11in.y"], ["pt.a11.x","pt.a11.y"] ],
            ["L", ["pt.a10.x","pt.a10.y"] ],
            ["C", ["pt.a10out.x","pt.a10out.y"], ["pt.a12in.x","pt.a12in.y"], ["pt.a12.x","pt.a12.y"] ],
            ["C", ["pt.a12out.x","pt.a12out.y"], ["pt.a8in.x","pt.a8in.y"], ["pt.a8.x","pt.a8.y"] ],
            ["C", ["pt.a8out.x","pt.a8out.y"], ["pt.a7in.x","pt.a7in.y"], ["pt.a7.x","pt.a7.y"] ],
            ["C", ["pt.a7out.x","pt.a7out.y"], ["pt.a6in.x","pt.a6in.y"], ["pt.a6.x","pt.a6.y"] ],
            ["L", ["pt.a5.x","pt.a5.y"] ],
            ["C", ["pt.a5out.x","pt.a5out.y"],["pt.a1in.x","pt.a1in.y"],["pt.a1.x","pt.a1.y"] ],
            ["z"]
          ],
          "drawattr": {},
          "appearanceattr": { "stroke-width":"3", "stroke":"#000000", "fill":"none" }
        }
    ],

    "construction": [
        { "id" : "Center Back line",
          "type": "path",
          "d" : [
            ["M", ["pt.a11.x","pt.a1.y"] ],
            ["L", ["pt.a1.x","pt.a1.y"] ]
          ],
          "drawattr": {},
          "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
        },
        { "id": "Back Cervicale line",
          "type": "path",
          "d": [
            ["M", ["pt.a1.x","pt.a1.y"] ],
            ["L", ["pt.a11.x","pt.a1.y"] ]
          ],
          "drawattr": {},
          "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
        },
        { "id": "Upper Back line",
          "type": "path",
          "d": [
            ["M", ["pt.a1.x","pt.a7.y"] ],
            ["L", ["pt.a11.x","pt.a7.y"] ]
          ],
          "drawattr": {},
          "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
        },
        { "id": "Back Upper Bust/Chest line",
          "type": "path",
          "d": [
            ["M", ["pt.a1.x","pt.a8.y"] ],
            ["L", ["pt.a11.x","pt.a8.y"] ]
          ],
          "drawattr": {},
          "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
        },
        { "id": "Back Waist line",
          "type": "path",
          "d": [
            ["M", ["pt.a1.x","pt.a2.y"] ],
            ["L", ["pt.a11.x","pt.a2.y"] ]
          ],
          "drawattr": {},
          "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
        },
        { "id": "Back Hip line",
          "type": "path",
          "d": [
            ["M", ["pt.a1.x","pt.a4.y"] ],
            ["L", ["pt.a11.x","pt.a4.y"] ]
          ],
          "drawattr": {},
          "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
        },
        { "id": "Ankle line",
          "type": "path",
          "d": [
            ["M", ["pt.a1.x","pt.a3.y"] ],
            ["L", ["pt.a11.x","pt.a3.y"] ]
          ],
          "drawattr": {},
          "appearanceattr": { "stroke-width":"1", "stroke":"#555555", "fill":"none" }
        }
    ],

    "points": {
      "a1": { "x": "0", "y": "0" },
      "a2": { "x": "0", "y": "1*meas.center_back_waist_length" },
      "a3": { "x": " -0.05*meas.back_waist_width", "y": "pt.a2.y + 1*meas.waist_to_hip_height + 1*meas.hip_height - 1*meas.ankle_height" },
      "a4" : "onLine(pt.a2, pt.a3, 1*meas.waist_to_hip_height)" ,
      "a5" : "onCircleAtX(pt.a2, 1*meas.center_back_waist_to_neck_point_balance, 0.6*meas.neck_width, 'y1 < y2')" ,
      "a6": "intersectCircles(pt.a5, 1*meas.shoulder_length, pt.a1, 1*meas.cervicale_to_shoulder_point_balance, 'y1 > y2')",
      "a7": { "x": "0.55*meas.upper_back_width", "y": "0.5*meas.Scye_depth" },
      "a8": { "x": "0.55*meas.back_bust_width", "y": "1.10*meas.Scye_depth" },
      "a9" : "onCircleAtX(pt.a8, 1*meas.side_waist_length, 0.55*meas.back_waist_width, 'y1 > y2')",
      "a10" : "polar(pt.a4, 0.6*meas.back_hip_width, angleBetween(pt.a2, pt.a9))",
      "a11" : "polar(pt.a10, dist(pt.a4, pt.a3), angleBetween(pt.a9, pt.a10))",
      "a12" : "right(pt.a9, 0.05*dist(pt.a2, pt.a9))",
      "a1in" : "right(pt.a1, dist(pt.a1, pt.a5)/5)",
      "a2out" : "down(pt.a2, dist(pt.a2, pt.a4)/3)",
      "a3out" : "polar(pt.a3, dist(pt.a3, pt.a11)/3, angleBetween(pt.a4, pt.a3) - radians(90) )",
      "a4in" : "onLine(pt.a3, pt.a4, dist(pt.a2, pt.a4)/3)",
      "a5out" : "polar(pt.a5, dist(pt.a1,pt.a5)/6, angleBetween(pt.a5, pt.a6) + radians(90) )",
      "a6in" : "polar(pt.a6, dist(pt.a6, pt.a7)/3, angleBetween(pt.a5, pt.a6) + radians(90) )",
      "a8in" : "polar(pt.a8, dist(pt.a8, pt.a12)/3, angleBetween(pt.a8, pt.a12))",
      "a8out" : "left(pt.a8, dist(pt.a8, pt.a7)/3)",
      "a7in"  : "polar(pt.a7, dist(pt.a8,pt.a7)/3.0, 0.5*(angleBetween(pt.a6in,pt.a7) + angleBetween(pt.a7,pt.a8out)))",
      "a7out" : "polar(pt.a7, dist(pt.a7,pt.a6)/3.0, angleBetween(pt.a7in, pt.a7))",
      "a10out" : "onLine(pt.a10, pt.a12, dist(pt.a10, pt.a12)/3)",
      "a11in" : "polar(pt.a11, dist(pt.a11, pt.a3)/3, angleBetween(pt.a10, pt.a11) + radians(90))",
      "a12in" : "down(pt.a12, dist(pt.a12, pt.a10)/3)",
      "a12out" : "up(pt.a12, dist(pt.a8, pt.a12)/3)"
    }
  }
}
